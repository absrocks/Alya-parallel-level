<item>
         <widget class="QGroupBox" name="#itemName">
          <property name="sizePolicy">
           <sizepolicy hsizetype="Preferred" vsizetype="Preferred">
            <horstretch>0</horstretch>
            <verstretch>0</verstretch>
           </sizepolicy>
          </property>
          <property name="minimumSize">
           <size>
            <width>0</width>
            <height>0</height>
           </size>
          </property>
          <property name="title">
           <string>#itemTitle</string>
          </property>
	  <property name="checkable">
           <bool><<checkable>></bool>
          </property>
          <layout class="QVBoxLayout" name="#itemLayoutName">
           #items
          </layout>
          <zorder/>
          <!--zorder>layoutWidget</zorder-->          
         </widget>
</item>
