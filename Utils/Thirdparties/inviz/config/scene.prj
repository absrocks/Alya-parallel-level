#! index_app_project 0
# -*- mode: Conf; coding: utf-8; -*-
#
# Options are defined one per line by <option-name> = <value>
# All other lines are either comments or are used in generating documentation.
# A single pound sign (#) followed by a space is a comment and is ignored.
# Two pound signs (##) specify documentation text for the options that follow.
# Two keywords are used in formatting the documentation:
# A line beginning with #section defines a new group of options.
# Single quotes (') define fixed font; double ('') defines italic.
# A * creates an item in a bullet list.
# A & creates an item in a numbered list.
# A line containing only ## generates a line break.
# Blank lines are ignored.

#------------------------------------------------------------
#section General scene settings

## Window resolution setting
index::canvas_resolution = 1024 1024
index::image_file_canvas_resolution = 2048 2048

## Global region of interest (XYZ)
index::region_of_interest = 0 0 0 2039 2039 2039

## Rendering seismic volume data.
index::is_render_seismic_volume = yes

## Adds a note related to the used dataset(s). The note
## will be displayed at the bottom of the canvas of the demo viewer.
## The note may be left empty so that no note will be displayed.
## The note can be formatted using html.
#app::dataset_note = Dataset information

## Directory that contains dataset description files for use with the
## scene editor.
app::dataset_description_directory = dataset_descriptions

## Picking is enabled by default
#app::disable_pick_operations = yes

## Show pick point(small sphere) when picking operation is performed
#app::show_pick_point = no

#------------------------------------------------------------
#section Multi-view mode

## Define number of viewports. With the default value of 0 a single
## viewport covering the entire canvas is used.
app::viewport::count = 0
#app::viewport::count = 4

## Camera changes can be applied to all viewports or just to the active viewport's camera
app::viewport::share_camera = yes
## Enable to generate additional debug output
app::viewport::advisory = no

## Define some example viewport.
## The bounding boxes must be consistent with the canvas resolution.
app::viewport::0::anchor_position = 0 0
app::viewport::0::size = 512 512
app::viewport::1::anchor_position = 512 0
app::viewport::1::size = 512 512
app::viewport::2::anchor_position = 0 512
app::viewport::2::size = 512 512
app::viewport::3::anchor_position = 512 512
app::viewport::3::size = 512 512

## Create additional local scopes.
## By default all viewports will use the the global scope. When
## creating local scopes, they will be used by the viewports starting
## at viewport 1.
#app::local_scopes = 3

