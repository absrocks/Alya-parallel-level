  PHYSICAL_PROBLEM
    WALL_LAW:     VARIA,MULTI=2.0
    COUPLING
      NASTIN ALEFOR  1
      LEVELS ALEFOR  1
      TURBUL ALEFOR  1
    END_COUPLING
    GRAVITY:         On, NORM: 9.81,GX:0.0,GY:0.0,GZ:-1.0
    DENSITY=         1.0
    VISCOSITY=       1.0
  END_PHYSICAL_PROBLEM
  NUMERICAL_TREATMENT
    MESH
$      DIVISION = 1
    END_MESH
    WALL_DISTANCE
       ALGEBRAIC_SOLVER:        deflated_CG, ITERA= 1000,TOLER= 1e-8
       PRECONDITIONING:         diagonal
       CODES BOUNDARIES
          3      1
       END_CODES
    END_WALL_DISTANCE
  END_NUMERICAL_TREATMENT
  OUTPUT_&_POST_PROCESS
    MESH
    STEP_OVER: 20
$   POSTPROCESS NUMBERING
$   POSTPROCESS CODNO
    POSTPROCESS WALL_DISTANCE, STEPS=10
  END_OUTPUT_&_POST_PROCESS
