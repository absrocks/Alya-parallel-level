		<keyword attribute="vars" context="#stay" String="Variables" />
		<DetectChar attribute="String" context="string" char="&quot;" /> 
                <DetectChar attribute="Comment" context="Comment" char="$"/> 
		<DetectChar attribute="Comment" context="Comment" char="!"/>
		<StringDetect context="#stay" attribute="Keyword1" String="OUTPUT_&amp;_POST_PROCESS"/>
		<StringDetect context="#stay" attribute="Keyword1" String="END_OUTPUT_&amp;_POST_PROCESS"/>
		<StringDetect context="#stay" attribute="Operator" String="="/>
		<StringDetect context="#stay" attribute="Operator" String=","/>
		<StringDetect context="#stay" attribute="Operator" String=":"/>
		
		<RegExpr attribute="int" context="#stay" String="(\d+)(\&amp;)(\d+)" />
		<RegExpr attribute="int" context="#stay" String="(\&amp;)(\d+)" />
		<RegExpr attribute="int" context="#stay" String="(\d+)(\&amp;)" />
		
		<StringDetect context="#stay" attribute="Operator" String="&amp;"/>
		<RegExpr attribute="int" context="#stay" String="(\d+(\.\d+)?|\.\d+)([eE](\d+))" />
		<RegExpr attribute="int" context="#stay" String="(\d+(\.\d+)?|\.\d+)([eE][+-](\d+))" />
		<RegExpr attribute="int" context="#stay" String="(\d+(\.\d+)?|\.\d+)([eE][+-]?\d+)?(im)?(?=')" />
		<RegExpr attribute="int" context="#stay" String="(\d+(\.\d+)?|\.\d+)" />
		
	    </context>
	    
	    <context name="Comment" attribute="Comment" lineEndContext="#pop"/>
	  
	    <context attribute="String" lineEndContext="#stay" name="string" > 
		<keyword attribute="Function" context="#stay" String="Code_step" />
		<DetectChar attribute="String" context="#pop" char="&quot;" /> 
	    </context> 
	    
	</contexts> 
