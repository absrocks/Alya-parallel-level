$------------------------------------------------------------
PHYSICAL_PROBLEM                                             
  PROBLEM_DEFINITION
    MODEL  stdkepsilon $CFDWind2
    TEMPORAL_DERIVATIVES      On
    TEMPERATURE_COUPLING      Off
  END_PROBLEM_DEFINITION
  PROPERTIES
    L_MAXI 0.27000000E+02
    TURBULENT_PRANDTL    0.9
    REAL_PARAMETERS     1.0000   1.1674   1.4400   1.9200   0.0000   0.0900 $sigma_k, sigma_e, c1, c2, c3, cmu
    INTEGER_PARAMETERS   AUTOMATIC
  END_PROPERTIES
END_PHYSICAL_PROBLEM                                         
$------------------------------------------------------------
NUMERICAL_TREATMENT
  STRATEGY                   ASGS
  ELEMENT_LENGTH             Minimum
  TIME_INTEGRATION           Trapezoidal, ORDER: 1, EULER=0
  SAFETY_FACTOR              10.0 expon 1.15  maxim     40.0 mingl 40.0
  STEADY_STATE_TOLER         -1d-9  
  NORM_OF_CONVERGENCE        ALGEBRAIC
  RELAXATION_FACTOR          0.5    
  MAXIMUM_NUMBER_OF_IT       2
  INNER_ITERATIONS           2
  CONVERGENCE_TOLERANCE      1d-10  
  ALGEBRAIC_SOLVER           GMRES, KRYLOV=50, ITERA= 170, TOLER= 1e-12, ADAPTIVE, RATIO = 0.0005  FIXITY
  PRECONDITIONING            DIAGONAL
  CLIPPING                   ALMOST
END_NUMERICAL_TREATMENT
$------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  START_POSTPROCES_AT        STEP =0
  POSTPROCESS KEY            STEPS=100
  POSTPROCESS EPSILON        STEPS=100
$  POSTPROCESS OMEGA        STEPS=100
  $ POSTPROCESS TUR_VISCOSITY  STEPS=100
  WITNESS_POINTS
    KEY
    EPSILON
  END_WITNESS_POINTS
END_OUTPUT_&_POST_PROCESS
$------------------------------------------------------------
BOUNDARY_CONDITIONS
CODES, NODES
     5   3     $ WALL 
     3 & 5 3   $ WALL entry 
     4 & 5 3   $ WALL output 
     6   0 FIELD=2   $ top 
     6 & 3 1 FIELD=2 $ top entry 
     6 & 4 0 FIELD=2 $ top output       
     3   1 FIELD=2   $ entry 
     4   0 FIELD=2   $ output 
 END_CODES 
 CODES, NODES, VARIABLE=2 
     5     3     $ WALL 
     3 & 5 3   $ WALL entry 
     4 & 5 3   $ WALL output 
     6   0 FIELD=3   $ top 
     6 & 3 1 FIELD=3 $ top entry 
     6 & 4 0 FIELD=3 $ top output       
     3   1 FIELD=3   $ entry 
     4   0 FIELD=3   $ output 
 END_CODES 
 CODES, BOUNDARIES   $, VARIABLE=1  
     4                20      1000  
  END_CODES 
   INITIAL VALUE_FUNCTION=2, VARIABLE = 1  ! use field 2 for initial condition for k
   INITIAL VALUE_FUNCTION=3, VARIABLE = 2  ! use field 3 for initial condition for e
END_BOUNDARY_CONDITIONS
$------------------------------------------------------------
