$------------------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION
    TEMPORAL_DERIVATIVES:   Off
    CONVECTIVE_TERM:        Off
    CONDUCTION_TERM:        On
$    SOURCE:                 SPACE_TIME_FUNCTION: SOURCE
  END_PROBLEM_DEFINITION
END_PHYSICAL_PROBLEM
$------------------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:           ASGS
  ELEMENT_LENGTH:          Minimum 
  SAFETY_FACTOR=           50.0
  TRACKING:                TIME 
  TIME_INTEGRATION:        BDF, ORDER=1
  STEADY_STATE_TOLERANCE=  -1e-10  
  MAXIMUM_NUMBER_OF_ITER=  1
  RELAXATION_FACTOR=       1.0
  CONVERGENCE_TOLERANCE=   -1e-8
  ALGEBRAIC_SOLVER     
    SOLVER:                DEFLATED_CG
    CONVERGENCE:           ITERA=10, TOLER=1.0e-12
    OUTPUT:                CONVERGENCE
    PRECONDITIONER:        DIAGONAL
  END_ALGEBRAIC_SOLVER  
END_NUMERICAL_TREATMENT
$------------------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  POSTPROCESS TEMPERATURE
END_OUTPUT_&_POST_PROCESS
