$ mu = dynamic viscosity [m2/s] = [kg/m/s] / [kg/m3] 
$  v = mu / rho                                                -> mu = 1.788e-3   
$
$ Pr = mu * cp/ k = 1.788e-3 * rho * cp / 2.56 = 0.71          -> cp ~ 1016.56   
$
$ Re = rho * v * L / mu = v *  7e-3 * 1.177 / 1.788e-3 = 40.0  ->  v ~    
$ 
$ Ma = v/c = v / sqrt( gamma * Rair / Mair * T) = v / np.sqrt(1.4 * 8.314510/0.028 * 273)  
$            
PHYSICAL_PROBLEM
  $$ LOWMACH:
  PROPERTIES
    MATERIAL = 1
      DENSITY       = LOW_MACH, GMIXT 
     $DENSITY       = CONSTANT, PARAMETER =    1.177 
      VISCOSITY     = CONSTANT, PARAMETER =    1.788e-3 # Ns/m2   <-  2.09e-3
      SPECIFIC_HEAT = CONSTANT, PARAMETER = 1016.56     # J/kg/K  <-  869.665  
      CONDUCTIVITY  = CONSTANT, PARAMETER =    2.56     # W/M/K 
    END_MATERIAL
  END_PROPERTIES
  $
  COUPLING
    TEMPER, NASTIN = 1 1
    NASTIN, TEMPER = 1 1
    NASTIN, ALEFOR = 1 1
  END_COUPLING
  $
END_PHYSICAL_PROBLEM

NUMERICAL_TREATMENT
  MESH
   $DIVISION=1
  END_MESH
  $                                           _ tmax       _ initial value
  $                                          /            /
  SPACE_&_TIME_FUNCTIONS
    FUNCTION=INFLOW 
    ( 1.5 + 0.5*tanh( 0.5*(-10.0 + 20.0*t/0.66e-4) ) ) * 300.0 
    END_FUNCTION
    FUNCTION=X_MOTION 
     3.5e-3 * sin( 2 * 3.14159 * 7 * (t/5.0e-2) )
    END_FUNCTION
    FUNCTION=INLET 
    ( sin(2*3.14159/4*y/0.035)*0.1 + 1 ) * 1.0
    END_FUNCTION
  END_SPACE_&_TIME_FUNCTIONS
  $  
  ELSEST: ON
    STRATEGY: BIN
    NUMBER: 50,50,50
  END_ELSEST
  $ 
END_NUMERICAL_TREATMENT

OUTPUT_&_POST_PROCESS
  ON_LASTMESH
  STEPS = 10   
 $POSTPROCESS  CODNO 
 $POSTPROCESS  DENSI 

 WITNESS_POINTS, NUMBER=1  
   0.1    0.0 0.0
 END_WITNESS_POINTS

