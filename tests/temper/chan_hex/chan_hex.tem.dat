$------------------------------------------------------------------------
PHYSICAL_PROBLEM

   PROBLEM_DEFINITION
     TEMPORAL_DERIVATIVES:   ON
     CONVECTIVE_TERM:        ON
     CONDUCTION_TERM:        OFF
     SOURCE_TERMS:           OFF
   END_PROBLEM_DEFINITION

   PROPERTIES:
     $TURBULENT_PRANDTL = 0.7
   END_PROPERTIES

END_PHYSICAL_PROBLEM
$------------------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:             ENTROPY 
  ELEMENT_LENGTH:            Minimum
  TIME_INTEGRATION:          RUNGE , ORDER: 4
  SAFETY_FACTOR=             1.0

  STEADY_STATE_TOLERANCE=    -1e-5
  NORM_OF_CONVERGENCE:       L2
  MAXIMUM_NUMBER_OF_ITER=    1
  RELAXATION_FACTOR=         1.0
  CONVERGENCE_TOLERANCE=     1e-6

  ALGEBRAIC_SOLVER
    SOLVER:                  EXPLICIT, APPROXIMATE, ORDER=4
  END_ALGEBRAIC_SOLVER

END_NUMERICAL_TREATMENT
$------------------------------------------------------------------------
OUTPUT_&_POST_PROCESS

  START_POSTPROCES_AT       STEP =0
  POSTPROCESS TEMPE
  POSTPROCESS ENVIT

END_OUTPUT_&_POST_PROCESS
$------------------------------------------------------------------------
BOUNDARY_CONDITIONS, NONCONSTANT

 INITIAL , VALUE = 1

 CODES, NODES
   1   0  0.0
 END_CODES

END_BOUNDARY_CONDITIONS
