$------------------------------------------------------------------------
PHYSICAL_PROBLEM

   PROBLEM_DEFINITION
     TEMPORAL_DERIVATIVES:   ON
     CONVECTIVE_TERM:        ON
     CONDUCTION_TERM:        ON
     SOURCE_TERMS:           OFF
     REGIME:                 LOWMACH
   END_PROBLEM_DEFINITION

END_PHYSICAL_PROBLEM
$------------------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:           ASGS
  STABILIZATION:           CONSTANTS  1.0  3.0  1.0  
  TAU_STRATEGY:            INCLUDING_DT
  ELEMENT_LENGTH:          MINIMUM
  
  TIME_INTEGRATION:        TRAPEZOIDAL, ORDER: 1, EULER=5
  SAFETY_FACTOR=           1e30                            $ b) 
  STEADY_STATE_TOLERANCE= -1e-8  
  NORM_OF_CONVERGENCE:     L2   
  MAXIMUM_NUMBER_OF_ITER=  10   
  RELAXATION_FACTOR=       0.8
  CONVERGENCE_TOLERANCE=   1e-4   
  $ 
  ALGEBRAIC_SOLVER
    SOLVER:                GMRES, KRYLOV=90
    CONVERGENCE:           ITERA=1000, TOLER=1.0e-10
    OUTPUT:                CONVERGENCE
    PRECONDITIONER:        DIAGONAL
    OPTIONS:               FIXITY
  END_ALGEBRAIC_SOLVER
  $
END_NUMERICAL_TREATMENT
$------------------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  START_POSTPROCES_AT       STEP =0
  POSTPROCESS TEMPE
  POSTPROCESS ENTHA
  BOUNDARY_SET
    MEANH    $ mean heat flux across set
    DENSI    $ mean density on set
  END_BOUNDARY_SET
END_OUTPUT_&_POST_PROCESS
$------------------------------------------------------------------------
BOUNDARY_CONDITIONS $, NONCONSTANT

  PARAMETER
    INITIAL, CONSTANT = 600.0
  END_PARAM


 CODES, NODES
       1    0  -1.00      $$ Bottom
       2    1  960.00     $$ Right
       3    0  -1.00      $$ Top
       4    1  240.00     $$ Left
     1 & 2  1  960.00  
     1 & 4  1  240.00  
     2 & 3  1  960.00  
     3 & 4  1  240.00  
 END_CODES

END_BOUNDARY_CONDITIONS
$------------------------------------------------------------------------
