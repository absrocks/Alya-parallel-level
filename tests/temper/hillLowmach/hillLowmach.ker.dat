PHYSICAL_PROBLEM
  WALL_LAW:  REICHARDT, VARIABLE  MULTIPLIER= 2
  PROPERTIES
    DENSITY       = LOW_MACH
    VISCOSITY     = CONSTANT,  PARAMETERS= 1.823e-05
    SPECIFIC_HEAT = CONSTANT,  PARAMETERS= 1004.5 
    CONDUCTIVITY  = CONSTANT,  PARAMETERS= 0.025
    POROSITY      = CONSTANT, PARAMETER = 0.0
    TURBU_VISCO   = WALE , PARAMETER = 0.25
  END_PROPERTIES
  COUPLING
    TEMPER,  NASTIN = 1 1
    NASTIN,  TEMPER = 1 1
  END_COUPLING
END_PHYSICAL_PROBLEM
NUMERICAL_TREATMENT
  MESH
$    DIVISION=1
  END_MESH
  ELSEST: ON
    STRATEGY: BIN
    NUMBER: 50,50,50
  END_ELSEST
  WALL_DISTANCE
      ALGEBRAIC_SOLVER:        deflated_CG, ITERA= 1000,TOLER= 1e-9
      PRECONDITIONING:         diagonal
      CODES BOUNDARIES
         3      1
         6      1
         7      1
         8      1
      END_CODES
  END_WALL_DISTANCE
END_NUMERICAL_TREATMENT
OUTPUT_&_POST_PROCESS
      ON_LASTMESH
      STEPS=100
END_OUTPUT_&_POST_PROCESS
$-------------------------------------------------------------------
