NUMERICAL_TREATMENT 
  MESH
    DIVISION=1
    EXTENDED_RAPH: ON
  END_MESH
$  DIRECT_SOLVER: PASTIX
  ELSEST
    STRATEGY: BIN 
    NUMBER:   10,10,10
    DATAF:    LINKED_LIST
  END_ELSES
  SPACE_&_TIME_FUNCTIONS
    FUNCTION=SOURCE
      1.0
    END_FUNCTION
  END_SPACE_&_TIME_FUNCTIONS
END_NUMERICAL_TREATMENT  
