$------------------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION
    TEMPORAL_DERIVATIVES:   Off
    CONVECTIVE_TERM:        Off
    CONDUCTION_TERM:        On
    SOURCE_TERM:            SPACE_&_TIME_FUNCTION=SOURCE
  END_PROBLEM_DEFINITION
END_PHYSICAL_PROBLEM
$------------------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:           ASGS
  ELEMENT_LENGTH:          Minimum 
  SAFETY_FACTOR=           50.0
  STEADY_STATE_TOLERANCE=  -1e-10  
  MAXIMUM_NUMBER_OF_ITER=  1
  RELAXATION_FACTOR=       1.0
  CONVERGENCE_TOLERANCE=   1e-03

  ALGEBRAIC_SOLVER
      SOLVER:              CG
      CONVERGENCE:         ITERA=100, TOLER=1.0e-06
      OUTPUT:              CONVERGENCE
      PRECONDITIONER:      DIAGONAL
$      OPTION:              SAVE_KRYLOV
  END_ALGEBRAIC_SOLVER  
END_NUMERICAL_TREATMENT
$------------------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  START_POSTPROCES_AT      STEP  = 0
  POSTPROCESS TEMPERATURE, STEPS=1
END_OUTPUT_&_POST_PROCESS
$------------------------------------------------------------------------
BOUNDARY_CONDITIONS
  CODES, NODES
       1 1 0.0
      11 1 0.0
  END_CODES
END_BOUNDARY_CONDITIONS
$------------------------------------------------------------------------
