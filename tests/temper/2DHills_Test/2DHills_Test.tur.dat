BOUNDARY_CONDITIONS
CODES, NODES, VARIABLE=1
     5     0 0.0     FIELD=7   $ WALL
     3 & 5 7 0.0     FIELD=7   $ WALL entry
     4 & 5 0 0.0     FIELD=7   $ WALL output
     6     0 0.0     FIELD=7   $ top
     3 & 6 7 0.0     FIELD=7   $ top entry
     4 & 6 0 0.0     FIELD=7   $ top output
     3     7 0.0     FIELD=7   $ entry
     4     0 0.0     FIELD=7   $ output
 END_CODES
 CODES, NODES, VARIABLE=2
     5     0 0.0     FIELD=8   $ WALL
     3 & 5 7 0.0     FIELD=8   $ WALL entry
     4 & 5 0 0.0     FIELD=8   $ WALL output
     6     0 0.0     FIELD=8   $ top
     3 & 6 7 0.0     FIELD=8   $ top entry
     4 & 6 0 0.0     FIELD=8   $ top output
     3     7 0.0     FIELD=8   $ entry
     4     0 0.0     FIELD=8   $ output
 END_CODES
   INITIAL VALUE_FUNCTION=2, VARIABLE = 1  ! use field 2 for initial condition for k
