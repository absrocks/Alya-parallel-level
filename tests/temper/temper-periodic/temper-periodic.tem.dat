$------------------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION
    TEMPORAL_DERIVATIVES:   Off
    CONVECTIVE_TERM:        On
    CONDUCTION_TERM:        On
    SOURCE_TERM:            SPACE_AND_TIME_FUNCTION=HEAT_SOURCE
  END_PROBLEM_DEFINITION
END_PHYSICAL_PROBLEM
$------------------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:           ASGS
  ELEMENT_LENGTH:          Minimum 
  SAFETY_FACTOR=           50.0
  STEADY_STATE_TOLERANCE=  1e-6  
  MAXIMUM_NUMBER_OF_ITER=  1
  RELAXATION_FACTOR=       1.0
  CONVERGENCE_TOLERANCE=   1e-03
  ALGEBRAIC_SOLVER
      SOLVER:              GMRES, KRYLOV=10
      CONVERGENCE:         ITERA=1000, TOLER=1.0e-10
      OUTPUT:              CONVERGENCE
      PRECONDITIONER:      DIAGONAL, RIGHT
  END_ALGEBRAIC_SOLVER  
END_NUMERICAL_TREATMENT
$------------------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  START_POSTPROCES_AT      STEP  = 0
  POSTPROCESS TEMPERATURE, STEPS=1
END_OUTPUT_&_POST_PROCESS
$------------------------------------------------------------------------
BOUNDARY_CONDITIONS
  CODES, NODES
      1  0  0.0
  1 & 2  1  0.0
      2  1  0.0 
  2 & 3  1  0.0
      3  0  0.0
  3 & 4  1  0.0
      4  1  0.0
  4 & 1  1  0.0
  END_CODES
END_BOUNDARY_CONDITIONS
$------------------------------------------------------------------------
