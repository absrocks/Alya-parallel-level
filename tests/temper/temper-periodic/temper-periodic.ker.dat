$------------------------------------------------------------
PHYSICAL_PROBLEM
   PROPERTIES
     MATERIAL: 1
      DENSITY:       CONSTANT, PARAMETERS = 1.0
      SPECIFIC_HEAT: CONSTANT, PARAMETERS = 1.0
      CONDUCTIVITY:  CONSTANT, PARAMETERS = 0.01
     END_MATERIAL
   END_PROPERTIES
   VELOCITY: FUNCTION=5
END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT 
  MESH
$   DIVISION=1
  END_MESH
  ELSEST
    STRATEGY: BIN
    NUMBER:   100,33
    DATAF:    LINKED_LIST
  END_ELSEST
  SPACE_&_TIME_FUNCTIONS
    FUNCTION=HEAT_SOURCE
      1.0
    END_FUNCTION
  END_SPACE_&_TIME_FUNCTIONS

END_NUMERICAL_TREATMENT  
$------------------------------------------------------------
OUTPUT_&_POST_PROCESS  
  ON_LAST_MESH
  STEPS=1e6
END_OUTPUT_&_POST_PROCESS  
