$------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION       
    TEMPORAL_DERIVATIVES:     ON 
    CONVECTIVE_TERM:          NONCO
    VISCOUS_TERM:             FULL
    REGIME = LOWMACH 
    $$ TURBULENCE= LES_MODEL, WALE , PARAMETER = 0.25 
  END_PROBLEM_DEFINITION  

  PROPERTIES
    THERMODYN_PRESSURE = 101325.0
  END_PROPERTIES

END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:            OFF
  ELEMENT_LENGTH:           MINIMUM
  TIME_INTEGRATION:         RUNGE, ORDER = 4
  SAFETY_FACTOR             0.95

  STEADY_STATE_TOLER:       -1e-10
  MAXIMUM_NUMBER_OF_IT:     1
  CONVERGENCE_TOLERANCE:    1e-4
  NORM_OF_CONVERGENCE       LAGGED_ALGEBRAIC_RESIDUAL

  VECTORIZED_ASSEMBLY:      ON
$$  TIME_STRATEGY:            EIGENVALUE

  ALGORITHM:                FRACTIONAL_STEP
    GAMMA = 0.0
    ROTAT = 0.0
    CONSISTENT              $ Solves pressure equation in each RK substep
  END_ALGORITHM


  END_ALGORITHM

  MOMENTUM
    ALGEBRAIC_SOLVER
      SOLVER:               EXPLICIT, LUMPED
    END_ALGEBRAIC_SOLVER
  END_MOMENTUM

  CONTINUITY
    ALGEBRAIC_SOLVER
      SOLVER:               DEFLATED_CG, COARSE: SPARSE
      CONVERGENCE:          ITERA=300, TOLER=1.0e-10, ADAPTIVE, RATIO=0.001
      PRECONDITIONER:       DIAGONAL
    END_ALGEBRAIC_SOLVER
  END_CONTINUITY

  DIRICHLET: ALGORITHM

END_NUMERICAL_TREATMENT 
$------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  POSTPROCESS VELOC
  POSTPROCESS PRESSURE
END_OUTPUT_&_POST_PROCESS  
$------------------------------------------------------------
BOUNDARY_CONDITIONS
  PARAMETERS
     INITIAL:  VALUE = 5
  END_PARAMETERS
  CODES, NODES 
       3    01  0.00  0.00
     1 & 3  11  0.65  0.00
   $$    2    00  -1.0 -1.0 
  END_CODES
END_BOUNDARY_CONDITIONS 
