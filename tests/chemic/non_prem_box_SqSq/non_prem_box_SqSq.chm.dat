$------------------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION
    MODEL:            FLAMELET, NON_PREMIX, LES $$ PREMIX/NON-PREMIXED, LAMINAR/LES
    Yc_VARIANCE:      Yc_Yc $$ OFF, Ycv, Yc_Yc
    Z_VARIANCE:       Z_Z   $$ OFF, Zv,  Z_Z
    LOOKUP:           GAUSS  
    TEMPORAL:         ON
    CONVECTION:       ON, VELOC
    DIFFUSION:        ON
  END_PROBLEM_DEFINITION

  PROPERTIES:
    TURBULENT_SCHMIDT = 0.7

    REACTIONS, TABLE
      INCLUDE cam_diff_turbulent_table.dat
    END_REACTIONS

    MASS_FRACTIONS_EQUILIBRIUM:
      INCLUDE cam_diff_turbulent_scaling_gen0.dat
    END_MASS_FRACTIONS
  END_PROPERTIES
  
  FIELDS: TEMPE= 1, SPECI = 2

END_PHYSICAL_PROBLEM
$------------------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:           ASGS 
  ELEMENT_LENGTH:          MINIMUM
  TIME_INTEGRATION:        RUNGE,  ORDER: 3
  SAFETY_FACTOR=           1.0

  STEADY_STATE_TOLERANCE  = 1e-8
  NORM_OF_CONVERGENCE     = L2
  CONVERGENCE_TOLERANCE   = 1e-6

  ALGEBRAIC_SOLVER
    SOLVER:               EXPLICIT, LUMPED
  END_ALGEBRAIC_SOLVER

  NEGATIVE_CLIP = ON
  POSITIVE_CLIP = ON 

END_NUMERICAL_TREATMENT
$------------------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  START_POSTPROCES_AT       STEP =0
  POSTPROCESS CONCE
  POSTPROCESS SCONC
  POSTPROCESS MOLEC
  POSTPROCESS CONDU
  POSTPROCESS SPHEA
  POSTPROCESS SOURC
  POSTPROCESS XZR  
  POSTPROCESS XYR  
  POSTPROCESS XZS  
  POSTPROCESS XYS  
  BOUNDARY_SET
    MASS
    CONCE
  END_BOUNDARY_SET
  WITNESS_POINTS
    CONCE
    SCONC
    XYR
    XYS
    XZR
    XZS
  END_WITNESS_POINTS
END_OUTPUT_&_POST_PROCESS
$------------------------------------------------------------------------
BOUNDARY_CONDITIONS

 CODES, NODES, CLASS = 1
   1    1   0.0
 1 & 3  1   0.0
 END_CODES
 CODES, NODES, CLASS = 2
   1    1   0.0
