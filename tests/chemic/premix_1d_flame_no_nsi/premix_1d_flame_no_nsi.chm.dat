$------------------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION
    MODEL:            FLAMELET, PREMIX, LAMINAR $$ PREMIX/NON-PREMIXED, LAMINAR/LES
    Yc_VARIANCE:      OFF $$ OFF, Ycv, Yc_Yc
    Z_VARIANCE:       OFF $$ OFF, Zv, Z_Z
    LOOKUP:           GAUSS  $$ GAUSS / NODAL, RELATE to KERNEL PROPERITES
    TEMPORAL:         ON
    CONVECTION:       ON, VELOC
    DIFFUSION:        ON
  END_PROBLEM_DEFINITION

  PROPERTIES:
    THERMODYN_PRESSURE = 101325.0
    REACTIONS, TABLE
      INCLUDE CH4_Air_ER0.67_prem_turbulent_table.dat  
    END_REACTIONS
  END_PROPERTIES
  
  FIELDS: TEMPE= 1, SPECI = 2

END_PHYSICAL_PROBLEM
$------------------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:           OFF 
  ELEMENT_LENGTH:          Minimum
  TIME_INTEGRATION:        RUNGE,  ORDER: 3
  SAFETY_FACTOR=           1.0

  STEADY_STATE_TOLERANCE  = 1e-8
  NORM_OF_CONVERGENCE     = L2
  CONVERGENCE_TOLERANCE=   1e-6

  ALGEBRAIC_SOLVER
    SOLVER:               EXPLICIT, LUMPED
  END_ALGEBRAIC_SOLVER

  NEGATIVE_CLIP = ON
  POSITIVE_CLIP = ON 

END_NUMERICAL_TREATMENT
$------------------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  POSTPROCESS CONCE
  POSTPROCESS SCONC
  POSTPROCESS MOLEC
  POSTPROCESS CONDU
  POSTPROCESS SPHEA
  POSTPROCESS SOURC
  POSTPROCESS XYR  
  POSTPROCESS AVY
  POSTPROCESS AVDEN
  WITNESS_POINTS
    CONCE
    SCONC
    XYR
  END_WITNESS_POINTS
END_OUTPUT_&_POST_PROCESS
$------------------------------------------------------------------------
BOUNDARY_CONDITIONS

 CODES, NODES, CLASS = 1
   1 & 3    1   0.0
 END_CODES
 CODES, NODES, CLASS = 2
   1 & 3    1   0.0
 END_CODES

END_BOUNDARY_CONDITIONS
$------------------------------------------------------------------------
