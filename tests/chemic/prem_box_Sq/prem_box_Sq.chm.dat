$------------------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION
    MODEL:            FLAMELET, PREMIX, LES $$ PREMIX/NON-PREMIXED, LAMINAR/LES
    Yc_VARIANCE:      Yc_Yc $$ OFF, Ycv, Yc_Yc
    LOOKUP:           GAUSS  
    TEMPORAL:         ON
    CONVECTION:       ON, VELOC
    DIFFUSION:        ON
  END_PROBLEM_DEFINITION

  PROPERTIES:
    TURBULENT_SCHMIDT = 0.7

    REACTIONS, TABLE
      INCLUDE CH4_Air_ER0.67_prem_turbulent_table.dat  
    END_REACTIONS
  END_PROPERTIES
  
  FIELDS: TEMPE= 1, SPECI = 2

END_PHYSICAL_PROBLEM
$------------------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:           ASGS 
  ELEMENT_LENGTH:          MINIMUM
  TIME_INTEGRATION:        RUNGE,  ORDER: 3
  SAFETY_FACTOR=           1.0

  STEADY_STATE_TOLERANCE  = 1e-8
  NORM_OF_CONVERGENCE     = L2
  CONVERGENCE_TOLERANCE   = 1e-6

  ALGEBRAIC_SOLVER
    SOLVER:               EXPLICIT, LUMPED
  END_ALGEBRAIC_SOLVER

  NEGATIVE_CLIP = ON
  POSITIVE_CLIP = ON 

END_NUMERICAL_TREATMENT
$------------------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  START_POSTPROCES_AT       STEP =0
  POSTPROCESS CONCE
  POSTPROCESS SCONC
  POSTPROCESS XYR  
  POSTPROCESS XYS  
  WITNESS
    CONCE
    SCONC
  END_WITNESS
END_OUTPUT_&_POST_PROCESS
