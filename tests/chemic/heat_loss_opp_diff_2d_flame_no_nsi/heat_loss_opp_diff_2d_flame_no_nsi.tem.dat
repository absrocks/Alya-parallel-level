$------------------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION
    TEMPORAL_DERIVATIVES:   ON
    CONVECTIVE_TERM:        ON
    CONDUCTION_TERM:        ON
    REGIME :                LOWMACH, ENTHALPY   
    SOURCE_TERMS:           OFF
  END_PROBLEM_DEFINITION

  PROPERTIES:
    THERMODYN_PRESSURE = 101325.0
  END_PROPERTIES


END_PHYSICAL_PROBLEM
$------------------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:           ASGS 
  ELEMENT_LENGTH:          MINIMUM
  TIME_INTEGRATION:        RUNGE , ORDER: 3
  SAFETY_FACTOR=           0.8

  STEADY_STATE_TOLERANCE=  -1e-5
  NORM_OF_CONVERGENCE:     L2
  MAXIMUM_NUMBER_OF_ITER=  1
  RELAXATION_FACTOR=       1.0
  CONVERGENCE_TOLERANCE=   1e-6

  ALGEBRAIC_SOLVER
    SOLVER:               EXPLICIT, LUMPED
  END_ALGEBRAIC_SOLVER
END_NUMERICAL_TREATMENT
$------------------------------------------------------------------------
OUTPUT_&_POST_PROCESS
 START_POSTPROCES_AT       STEP = 0
 POSTPROCESS TEMPE
 POSTPROCESS ENTHA
END_OUTPUT_&_POST_PROCESS
