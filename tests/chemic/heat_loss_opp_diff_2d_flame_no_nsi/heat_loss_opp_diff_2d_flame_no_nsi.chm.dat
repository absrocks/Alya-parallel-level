$------------------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION
    MODEL:            FLAMELET, NON_PREMIX, LAMINAR, NON_ADIABATIC 
    Z_VARIANCE:       OFF   $$ ZV,  ZZ,   OFF
    Yc_VARIANCE:      OFF   $$ YCV, YCYC, OFF
    TEMPORAL:         ON
    CONVECTION:       ON, VELOC
    DIFFUSION:        ON
  END_PROBLEM_DEFINITION

  PROPERTIES:
	TAB_FRAMEWORK:  1
	POST_FRAMEWORK: 2
  END_PROPERTIES

  FIELDS: SPECIES = 1

END_PHYSICAL_PROBLEM
$------------------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:           ASGS
  ELEMENT_LENGTH:          MINIMUM
  TIME_INTEGRATION:        RUNGE, ORDER: 3
  SAFETY_FACTOR=           0.8

  STEADY_STATE_TOLERANCE=  1e-8
  NORM_OF_CONVERGENCE:     L2
  CONVERGENCE_TOLERANCE=   1e-6

  ALGEBRAIC_SOLVER
    SOLVER:               EXPLICIT, LUMPED
  END_ALGEBRAIC_SOLVER

  NEGATIVE_CLIP =         ON
  POSITIVE_CLIP =         ON 
END_NUMERICAL_TREATMENT
$------------------------------------------------------------------------
OUTPUT_&_POST_PROCESS
 START_POSTPROCES_AT       STEP = 0
  POSTPROCESS CONCE
  POSTPROCESS SCONC
  POSTPROCESS SOURC
  POSTPROCESS CONDU
  POSTPROCESS VISCO
  POSTPROCESS XZR
  POSTPROCESS POST_TABLE
  POSTPROCESS AV_PO_TABLE
  WITNESS 
    CONCE
    SCONC
  END_WITNESS
END_OUTPUT_&_POST_PROCESS
