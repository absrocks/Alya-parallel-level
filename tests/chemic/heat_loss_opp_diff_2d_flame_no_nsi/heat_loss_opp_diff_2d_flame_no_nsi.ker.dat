PHYSICAL_PROBLEM
  PROPERTIES
    MATERIAL = 1
      DENSITY       = LOWMA, GP_LOOKUP, UPDP1=ENDSTE, UPDP2=NEVER
      VISCOSITY     = CONST, PARAMETER = 2.0e-3, UPDP1=ENDSTE, UPDP2=NEVER
      SPECIFIC_HEAT = CP_GP_TAB, UPDP1=ENDSTE, UPDP2=NEVER
      CONDUCTIVITY  = CONST, PARAMETER = 2.0e-1, UPDP1=ENDSTE, UPDP2=NEVER
    END_MATERIAL

    MATERIAL = 2
      DENSITY       = LOWMA, GP_LOOKUP, UPDP1=ENDSTE, UPDP2=NEVER
      VISCOSITY     = MU_GP_TAB, UPDP1=ENDSTE, UPDP2=NEVER
      SPECIFIC_HEAT = CP_GP_TAB, UPDP1=ENDSTE, UPDP2=NEVER
      CONDUCTIVITY  = K_GP_TAB, UPDP1=ENDSTE, UPDP2=NEVER
    END_MATERIAL
  END_PROPERTIES

  GAS_CONSTANT    = 8.3144621

  COUPLING
    TEMPER, CHEMIC = 1 1
    CHEMIC, TEMPER = 1 1
  END_COUPLING

  VELOCITY: FIELD= 6

END_PHYSICAL_PROBLEM
$-------------------------------------------------------------------
NUMERICAL_TREATMENT
  
  LOOKUP_TABLES
    TABLE: 1, ORDER=1
      INCLUDE cam_diff_turbulent_table.dat
    END_TABLE
    TABLE: 2, ORDER=1
      INCLUDE cam_diff_turbulent_scaling_gen0.dat
    END_TABLE
    TABLE: 3, ORDER=1
      INCLUDE cam_diff_turbulent_scaling_gen1.dat
    END_TABLE
    TABLE: 4, ORDER=1
      INCLUDE cam_diff_turbulentPost_table.dat
    END_TABLE

    FRAMEWORK: 1
      MAIN_TABLE    = 1
      SCALING
        ZMEAN UNSCALED
        ZVAR  OFF
        CMEAN TABLE 2
        IMEAN TABLE 3
      END_SCALING
    END_FRAMEWORK

    FRAMEWORK:2 
      MAIN_TABLE    = 4
      SCALING
        ZMEAN UNSCALED
        ZVAR  OFF
        CMEAN TABLE 2
        IMEAN CONST=1.0
      END_SCALING
    END_FRAMEWORK
  END_LOOKUP_TABLES
  
  ELSEST: ON
    STRATEGY: BIN
    NUMBER: 50,50,50
  END_ELSEST

  SPACE_&_TIME_FUNCTIONS
    FUNCTION=BODYTEMP
      400.0 + 300.0 * exp((y+2e-3)/(5e-3+2e-3))
    END_FUNCTION
  END_SPACE_&_TIME_FUNCTIONS
END_NUMERICAL_TREATMENT
$-------------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  ON_LASTMESH
  STEPS = 5

  POSTP VELOC

  WITNESS_POINTS, NUMBER= 1
     0.0   0.0
  END_WITNESS_POINTS

END_OUTPUT_&_POST_PROCESS
