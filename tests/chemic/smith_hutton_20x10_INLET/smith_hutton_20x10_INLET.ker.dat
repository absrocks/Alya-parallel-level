$------------------------------------------------------------
PHYSICAL_PROBLEM
  PROPERTIES
    MATERIAL = 1
      DENSITY:        CONSTANT,  PARAMETERS = 1.0
      CONDUCTIVITY:   CONSTANT,  PARAMETERS = 0.001
      SPECIFIC_HEAT:  CONSTANT,  PARAMETERS = 1.0
    END_MATERIAL
  END_PROPERTIES

  GAS_CONSTANT=   8.3144621

  VELOCITY: FIELD = 7
 
END_PHYSICAL_PROBLEM
$------------------------------------------------------------
NUMERICAL_TREATMENT
  ELSEST: ON
    STRATEGY: BIN
    NUMBER: 50,50,50
  END_ELSEST

  SPACE_&_TIME_FUNCTIONS  $$ 1+tanh(alpha * (2x+1))
    FUNCTION=INLET
      1.0 + tanh(10.0 * (1.0 + 2.0*x))
    END_FUNCTION
  END_SPACE_&_TIME_FUNCTIONS

END_NUMERICAL_TREATMENT  
$------------------------------------------------------------
OUTPUT_&_POST_PROCESS  
  ON_LAST_MESH
  STEPS=50
END_OUTPUT_&_POST_PROCESS  
$------------------------------------------------------------
