$------------------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION
    MODEL:            FLAMELET, NON_PREMIX, LAMINAR
    TEMPORAL:         ON
    CONVECTION:       ON, VELOC
    DIFFUSION:        OFF
  END_PROBLEM_DEFINITION

  PROPERTIES:
    THERMODYNAMIC_PRESSURE = 101325.0

    REACTIONS, TABLE
      INCLUDE ./DLR_diff_ext_Le1_database.dat
    END_REACTIONS

    MASS_FRACTIONS_EQUILIBRIUM:
      INCLUDE ./DLR_diff_ext_Le1_Z_dependent_data.dat
    END_MASS_FRACTIONS

  END_PROPERTIES

  FIELDS: TEMPE = 1, SPECIES = 2

END_PHYSICAL_PROBLEM
$------------------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:           GALERKIN 
  ELEMENT_LENGTH:          MINIMUM
  TIME_INTEGRATION:        RUNGE , ORDER: 3
  SAFETY_FACTOR=           1.0

  STEADY_STATE_TOLERANCE = -1e-8
  NORM_OF_CONVERGENCE    =    L2
  CONVERGENCE_TOLERANCE  =  1e-6

  ALGEBRAIC_SOLVER
    SOLVER:                EXPLICIT, LUMPED
  END_ALGEBRAIC_SOLVER

END_NUMERICAL_TREATMENT
$------------------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  POSTPROCESS CONCE
END_OUTPUT_&_POST_PROCESS
