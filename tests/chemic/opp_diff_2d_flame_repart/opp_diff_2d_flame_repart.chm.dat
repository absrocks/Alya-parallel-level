$------------------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION
    MODEL:            FLAMELET, NON_PREMIX, LAMINAR   
    Z_VARIANCE:       OFF   $$ ZV,  ZZ,   OFF
    Yc_VARIANCE:      OFF   $$ YCV, YCYC, OFF
    TEMPORAL:         ON
    CONVECTION:       ON, VELOC
    DIFFUSION:        ON
  END_PROBLEM_DEFINITION

  PROPERTIES:
    LAW_DIFFUSION:  TURBULENT_SCHMIDT = 0.7

    REACTIONS, TABLE
      INCLUDE ./cam_diff_turbulent_table.dat
    END_REACTIONS

    MASS_FRACTIONS_EQUILIBRIUM:
      INCLUDE ./cam_diff_turbulent_scaling_gen0.dat
    END_MASS_FRACTIONS

  END_PROPERTIES

  FIELDS: SPECIES = 1

END_PHYSICAL_PROBLEM
$------------------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:           ASGS
  ELEMENT_LENGTH:          MINIMUM
  TIME_INTEGRATION:        RUNGE, ORDER: 3
  SAFETY_FACTOR=           100.0

  STEADY_STATE_TOLERANCE=  1e-8
  NORM_OF_CONVERGENCE:     L2
  CONVERGENCE_TOLERANCE=   1e-6

  ALGEBRAIC_SOLVER
    SOLVER:               EXPLICIT, LUMPED
  END_ALGEBRAIC_SOLVER

  NEGATIVE_CLIP =         ON
  POSITIVE_CLIP =         ON 
END_NUMERICAL_TREATMENT
$------------------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  POSTPROCESS CONCE
  POSTPROCESS SCONC
  POSTPROCESS SOURC
  POSTPROCESS CONDU
  POSTPROCESS VISCO
  POSTPROCESS XZR
  WITNESS 
    CONCE
	 SCONC
  END_WITNESS
END_OUTPUT_&_POST_PROCESS
