$------------------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION
    MODEL:            SPRAY, LAMINAR
    SPRAY:            LEVELS
    TEMPORAL:         ON
    CONVECTION:       ON, VELOC
    DIFFUSION:        OFF
  END_PROBLEM_DEFINITION

  PROPERTIES
  END_PROPERTIES

  DROPLET_IDENTIFICATION
    LEVEL_SET_THRESHOLD = 0.5
    COMPACTNESS_LIMIT = 0.0
    MAX_DIAMETER = 100000.0
   $$ H_FACTOR = 4.0
  END_DROPLET_IDENTIFICATION

  FIELDS: SPECIES = 1

END_PHYSICAL_PROBLEM
$------------------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:           GALER
  ELEMENT_LENGTH:          MINIMUM
  TIME_INTEGRATION:        RUNGE, ORDER: 3
  SAFETY_FACTOR=           0.2

  STEADY_STATE_TOLERANCE=  -1e-8
  NORM_OF_CONVERGENCE:     L2
  CONVERGENCE_TOLERANCE=   1e-4

  ALGEBRAIC_SOLVER
    SOLVER:                EXPLICIT, LUMPED
  END_ALGEBRAIC_SOLVER

END_NUMERICAL_TREATMENT
$------------------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  START_POSTPROCES_AT       STEP = 0
  POSTPROCESS CONCE
  WITNESS
