$------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION       
    TEMPORAL_DERIVATIVES:     ON 
    CONVECTIVE_TERM:          NONCO
    VISCOUS_TERM:             FULL
    TURBULENCE=               LES_MODEL, VRMAN , PARAMETER = 0.1
    SURFACE:                  ELSA, COEFF = 0.01
  END_PROBLEM_DEFINITION  

END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:            OFF
  ELEMENT_LENGTH:           MINIMUM
  TIME_INTEGRATION:         RUNGE, ORDER = 3
  SAFETY_FACTOR             0.2

  STEADY_STATE_TOLER:       -1e-10
  MAXIMUM_NUMBER_OF_IT:     1
  CONVERGENCE_TOLERANCE:    1e-4
  NORM_OF_CONVERGENCE       LAGGED_ALGEBRAIC_RESIDUAL

  VECTORIZED_ASSEMBLY:      ON

  ALGORITHM:                FRACTIONAL_STEP
    GAMMA = 0.0
  END_ALGORITHM

  MOMENTUM
    ALGEBRAIC_SOLVER
      SOLVER:               EXPLICIT, LUMPED
    END_ALGEBRAIC_SOLVER
  END_MOMENTUM

  CONTINUITY
    ALGEBRAIC_SOLVER
      SOLVER:               DEFLATED_CG, COARSE: SPARSE
      CONVERGENCE:          ITERA=100, TOLER=1.0e-10, ADAPTIVE, RATIO=0.001
      PRECONDITIONER:       DIAGONAL
    END_ALGEBRAIC_SOLVER
  END_CONTINUITY

  DIRICHLET : ALGORITHM

END_NUMERICAL_TREATMENT 
$------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  START_POSTPROCES_AT       STEP =0
  POSTPROCESS VELOC
  WITNESS_POINTS
    VELOX
    VELOY
    VELOZ
    PRESSURE
    S11
    S22
    S33
    S12
    S13
    S23
    TURBU
 END_WITNESS_POINTS
END_OUTPUT_&_POST_PROCESS  
$------------------------------------------------------------
BOUNDARY_CONDITIONS
  PARAMETERS
    VARIATION:    NON_CONSTANT
    INITIAL:      VALUE = 5
  END_PARAMETERS
  CODES, NODES 
        1  111  1.0  0.0  0.0, SPACE_TIME_FUNCTION=VEL_IN  $ LEFT
        2  000  0.0  0.0  0.0                              $ RIGHT
        3  100  0.0  0.0  0.0, AXES=LOCAL                  $ WALL
     1 & 3 111  0.0  0.0  0.0
     2 & 3 000  0.0  0.0  0.0
  END_CODES
END_BOUNDARY_CONDITIONS 
