$------------------------------------------------------------
PHYSICAL_PROBLEM
$------------------------------------------------------------
PROBLEM_DEFINITION

  STARTING_POTENTIAL, FIELD
    CURDENSITY:  FIELD= 3
  END_STARTING_POTENTIAL

  PSEUDO_ECG= ON
    NUMBER_ROOT=  3
    FREQUENCY=  10
    COORDS
     0 -1  1
     0  1  1 
     0  1 -1
  END_PSEUDO

$------------------------------------------------------------
END_PROBLEM_DEFINITION
$------------------------------------------------------------
PROPERTIES

CM_CONST=                 1.0
PARAMETERS
  ISOCHRONES HIGHER 0.0
END_PARAMETERS
$---------------------------------------------------$
MATERIAL=1 $$ ENDOCARDIUM

CONTINUUM_MODEL
  IN_DIFFUSION= 0.001171 0.00039033 0.0039033
ENDCONTINNUM_MODEL

CELL_MODEL:   NOMODEL
  CM_CONST=  1.0 $[muF/cm2]
END_CELL_MODEL

END_MATERIAL
$---------------------------------------------------$
MATERIAL=2 $$ HETEROGENEOUS SCAR

CONTINUUM_MODEL
  IN_DIFFUSION= 0.001171 0.00039033 0.0039033
ENDCONTINNUM_MODEL

CELL_MODEL:   NOMODEL
  CM_CONST=  1.0 $[muF/cm2]
END_CELL_MODEL

END_MATERIAL
$---------------------------------------------------$
MATERIAL=3 $$ DENSE SCAR

CONTINUUM_MODEL
  IN_DIFFUSION= 0.001171 0.00039033 0.0039033
  CM_CONST=  1.0 $[muF/cm2]
  INITIAL_VOLTAGE= -86.1282
ENDCONTINNUM_MODEL

CELL_MODEL:   NOMODEL
  CM_CONST=  1.0 $[muF/cm2]
END_CELL_MODEL

END_MATERIAL
$---------------------------------------------------$
MATERIAL=4 $$ MYOCARDIUM 

CONTINUUM_MODEL

IN_DIFFUSION= 0.001171 0.00039033 0.00039033

ENDCONTINNUM_MODEL

CELL_MODEL:   OHARA INA_PASSINI $  TTHET
  CM_CONST=  1.0 $[muF/cm2]
END_CELL_MODEL

INI_CELL, HETEROGENEOUS
  INCLUDE parametersOHR_new.txt $$TTHET parameters w/ male/female phenotype
END_INI_CELL
END_MATERIAL
$---------------------------------------------------$

END_PROPERTIES


$------------------------------------------------------------
  FIBER_MODEL:              FIELD=1
  CELL_TYPE:                FIELD=2
$------------------------------------------------------------
END_PHYSICAL_PROBLEM
$------------------------------------------------------------
NUMERICAL_TREATMENT
   CONSTANT_MATRIX
   SAFETY_FACTOR=           1.0

 TIMETREATMENT: IMPLICIT, CRANK
 ALGEBRAIC_SOLVER     
    SOLVER:                CG
    CONVERGENCE:           ITERA=1000, TOLER=1.0e-5, ADAPTIVE, RATIO=0.01
    OUTPUT:                CONVERGENCE
    PRECONDITIONER:        DIAGONAL
 END_ALGEBRAIC_SOLVER        
END_NUMERICAL_TREATMENT
$------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  START_POSTPROCESS_AT STEP = 0

  $POSTPROCESS INTRA STEPS= 100 $000
  $POSTPROCESS ISOCHRONES, STEPS= 224000

  SAVE_CONVERGENCE CELL_MODEL 

  WITNESS_POINTS
   INTRA
  END_WITNESS_POINTS
  
END_OUTPUT_&_POST-PROCESS
$------------------------------------------------------------
BOUNDARY_CONDITIONS, CONSTANT  
END_BOUNDARY_CONDITIONS
$------------------------------------------------------------
