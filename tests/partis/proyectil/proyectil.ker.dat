$-------------------------------------------------------------------
PHYSICAL_PROBLEM
  PROPERTIES
    MATERIAL: 1
       DENSITY:       Constant, VALUE=1.0
       VISCOSITY:     Constant, VALUE=1.0
       CONDUCTIVITY:  Constant, VALUE=1.0
       SPECIFIC_HEAT: Constant, VALUE=1.0
    END_MATERIAL
  END_PROPERTIES
 VELOCITY FUNCTION=3
 GRAVITY:     NORM=9.81,GX=0.0, GY=-1.0
END_PHYSICAL_PROBLEM
$-------------------------------------------------------------------
NUMERICAL_TREATMENT 
  ELSEST
    STRATEGY:    Bin
    NUMBER_BINS= 20,20,20
  END_ELSEST
  MESH
    MULTIPLICATION= 0
  END_MESH
  WALL_DISTANCE                                 
   ALGEBRAIC_SOLVER    DEFLATED_CG, ITERA= 5000,TOLER= 1e-9, ADAPTIVE, RATIO=1e-9
   PRECONDITIONING     DIAGONAL                
   CODES, BOUNDARIES
     3  1                   
    END_CODES                                  
  END_WALL_DISTANCE   

END_NUMERICAL_TREATMENT  
$-------------------------------------------------------------------
OUTPUT_&_POST_PROCESS  
  On_last_mesh
  $ STEPS = 1
END_OUTPUT_&_POST_PROCESS  
$-------------------------------------------------------------------
