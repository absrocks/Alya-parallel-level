PHYSICAL_PROBLEM
  PROPERTIES
    MATERIAL: 1
      DENSITY:   CONSTANT, VALUE=1.0
      VISCOSITY: CONSTANT: VALUE=0.01
    END_MATERIAL
  END_PROPERTIES
  VELOCITY: FUNCTION=2
  GRAVITY:  NORM=9.81,GX=0.0,GY=-1.0
END_PHYSICAL_PROBLEM  
