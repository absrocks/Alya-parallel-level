NUMERICAL_TREATMENT 
  MESH
    DIVISION=0
  END_MESH
  WALL_DISTANCE
      ALGEBRAIC_SOLVER:        CG, ITERA= 1000,TOLER= 1e-8
      PRECONDITIONING:         DIAGONAL
      CODES, NODES 
         1      1
         2      1
      END_CODES
  END_WALL_DISTANCE
  ELSEST
$    STRATEGY: OCT_TREE
$    MAXIMUM:  10
    STRATEGY: BIN
    NUMBER:   10,10,10
  END_ELSEST 
END_NUMERICAL_TREATMENT  
$------------------------------------------------------------
OUTPUT_&_POST_PROCESS  
  ON_ORIGINAL_MESH
  STEPS=1
END_OUTPUT_&_POST_PROCESS  
