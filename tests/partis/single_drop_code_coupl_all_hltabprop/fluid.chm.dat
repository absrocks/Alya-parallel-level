$------------------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION
    MODEL:            FLAMELET, NON_PREMIX, LES, NON_ADIABATIC 
    Z_VARIANCE:       OFF   
    Yc_VARIANCE:      OFF   
    TEMPORAL:         ON
    CONVECTION:       ON, VELOC
    DIFFUSION:        ON
  END_PROBLEM_DEFINITION

  PROPERTIES:
    TURBULENT_SCHMIDT  = 0.7

    REACTIONS, TABLE
      INCLUDE water_turbulent_table.dat 
    END_REACTIONS

    MASS_FRACTIONS_EQUILIBRIUM:
      INCLUDE water_turbulent_scaling_gen0.dat 
    END_MASS_FRACTIONS

	 HEAT_LOSS_TABLE
	   INCLUDE water_turbulent_scaling_gen1.dat
	 END_HEAT_LOSS
  END_PROPERTIES

  FIELDS: TEMPE= 1, SPECIES = 2

END_PHYSICAL_PROBLEM
$------------------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:           ASGS 
  ELEMENT_LENGTH:          MINIMUM
  TIME_INTEGRATION:        RUNGE, ORDER: 3
  SAFETY_FACTOR=           0.95

  STEADY_STATE_TOLERANCE=  1e-8
  NORM_OF_CONVERGENCE:     L2
  MAXIMUM_NUMBER_OF_ITER=  1
  CONVERGENCE_TOLERANCE=   1e-6

  ALGEBRAIC_SOLVER
    SOLVER:               EXPLICIT, LUMPED
  END_ALGEBRAIC_SOLVER

$  NEGATIVE_CLIP =         ON
$  POSITIVE_CLIP =         ON 
END_NUMERICAL_TREATMENT
$------------------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  START_POSTPROCES_AT       STEP = 0
  POSTPROCESS CONCE
  POSTPROCESS SCONC
  POSTPROCESS MASSK
  WITNESS 
    CONCE
  END_WITNESS
  ELEMENT_SETS
    MASS
    CONCE
  END_ELEMENT_SETS
END_OUTPUT_&_POST_PROCESS
$------------------------------------------------------------------------
BOUNDARY_CONDITIONS
 CODES, NODES, CLASS = 1
