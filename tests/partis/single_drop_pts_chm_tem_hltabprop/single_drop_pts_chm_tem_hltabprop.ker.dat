$------------------------------------------------------------
PHYSICAL_PROBLEM
  PROPERTIES
    MATERIAL: 1
      DENSITY       = LOWMA, GP_LOOKUP, UPDP1=ENDSTE, UPDP2=NEVER
      VISCOSITY     = MU_GP_TAB, UPDP1=ENDSTE, UPDP2=NEVER
      SPECIFIC_HEAT = CP_GP_TAB, UPDP1=ENDSTE, UPDP2=NEVER
      CONDUCTIVITY  = K_GP_TAB, UPDP1=ENDSTE, UPDP2=NEVER
    END_MATERIAL
  END_PROPERTIES

  GAS_CONSTANT    = 8.3144621

  VELOCITY:      NULL
  GRAVITY:       NORM=9.81, GX=0.0,GY=-1.0,GZ=0.0

  COUPLING
    PARTIS, CHEMIC = 1 1
    CHEMIC, PARTIS = 1 1
    PARTIS, TEMPER = 1 1
    TEMPER, PARTIS = 1 1
    CHEMIC, TEMPER = 1 1
    TEMPER, CHEMIC = 1 1
  END_COUPLING

END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT 
  ELSEST
    STRATEGY: BIN
    NUMBER:   10,10,10
  END_ELSEST

  MESH
    DIVISION=0
  END_MESH

  LOOKUP_TABLES
    TABLE: 1
      INCLUDE water_turbulent_table.dat
    END_TABLE
    TABLE: 2
      INCLUDE water_turbulent_scaling_gen0.dat
    END_TABLE
    TABLE: 3
      INCLUDE water_turbulent_scaling_gen1.dat
    END_TABLE

    FRAMEWORK: 1
      MAIN_TABLE = 1
      SCALING
        ZMEAN UNSCALED
        ZVAR  OFF
        CMEAN TABLE 2
        CVAR  OFF
        IMEAN TABLE 3
      END_SCALING
    END_FRAMEWORK
  END_LOOKUP_TABLES


END_NUMERICAL_TREATMENT  
$------------------------------------------------------------
OUTPUT_&_POST_PROCESS  
  ON_LAST_MESH
  STEPS=10
END_OUTPUT_&_POST_PROCESS  
$------------------------------------------------------------
