$------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION       
    TEMPORAL_DERIVATIVES:     ON 
    CONVECTIVE_TERM:          NONCO 
    VISCOUS_TERM:             FULL
    REGIME =                  LOWMACH 
  END_PROBLEM_DEFINITION  
  PROPERTIES:
    THERMODYN_PRESSURE = 1.01325e5
  END_PROPERTIES
END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT 
  STABILIZATION:            OFF
  ELEMENT_LENGTH:           MINIMUM
  TIME_INTEGRATION:         RUNGE, ORDER= 3
  SAFETY_FACTOR             0.9

  STEADY_STATE_TOLER:       -1e-10
  MAXIMUM_NUMBER_OF_IT:     1
  CONVERGENCE_TOLERANCE:    1e-4
  NORM_OF_CONVERGENCE       LAGGED_ALGEBRAIC_RESIDUAL

  VECTORIZED_ASSEMBLY:      ON
  TIME_STRATEGY:            EIGENVALUE

  ALGORITHM:                FRACTIONAL_STEP
    GAMMA = 0.0
  END_ALGORITHM

  MOMENTUM
    ALGEBRAIC_SOLVER
      SOLVER:               EXPLICIT, LUMPED
    END_ALGEBRAIC_SOLVER
  END_MOMENTUM

  CONTINUITY
    ALGEBRAIC_SOLVER
      SOLVER:               DEFLATED_CG, COARSE: SPARSE
      CONVERGENCE:          ITERA=300, TOLER=1.0e-10, ADAPTIVE, RATIO=0.001
    END_ALGEBRAIC_SOLVER
  END_CONTINUITY

  DIRICHLET: ALGORITHM

END_NUMERICAL_TREATMENT  
$------------------------------------------------------------
OUTPUT_&_POST_PROCESS  
  POSTPROCESS VELOC
  POSTPROCESS PRESS
  POSTPROCESS MOMSK
  ELEMENT_SETS
    KINETIC_ENERGY
    MOMENTUM
  END_ELEMENT_SETS
END_OUTPUT_&_POST_PROCESS   
$------------------------------------------------------------
BOUNDARY_CONDITIONS
  PARAMETERS
    VARIATION:    NON_CONSTANT
    INITIAL:      VALUE=7
  END_PARAMETERS
  CODES, NODES
        1  111  0.01 0.0  0.0             $ LEFT
        2  000  0.0  0.0  0.0             $ RIGHT
        3  100  0.0  0.0  0.0, AXES=LOCAL $ WALL
    1 & 3  111  0.01 0.0  0.0
    2 & 3  000  0.0  0.0  0.0
  END_CODES
END_BOUNDARY_CONDITIONS  
$------------------------------------------------------------
