PHYSICAL_PROBLEM
   MAXIMUM = 1
   MOMENTUM_TRANSPORT:      ON   
   THERMODYNAMIC_TRANSPORT: ON
   TYPE= 1
      MOMENTUM_MODEL:       FORCE
      THERMODYNAMIC_MODEL:  ON, FLAMELET_TABLE
      BROWNIAN_MOTION:      OFF
      FORCES:               DRAG: SCHILLER_NEUMANN, GRAVITY
      PROPERTIES
        LIQ_NAME= NHEPT
        D=        60.0e-06
        TABLE
            INCLUDE ./cam_diff_database.dat
        END_TABLE
        YC_SCALING
            INCLUDE ./cam_diff_Z_dependent_data.dat 
        END_YC_SCALING
      END_PROPERTIES
  END_TYPE

  SINK_TERMS: MOMENTUM, HEAT , MASS

END_PHYSICAL_PROBLEM
