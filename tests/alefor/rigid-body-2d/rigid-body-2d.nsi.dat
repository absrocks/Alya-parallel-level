$------------------------------------------------------------
PHYSICAL_PROBLEM
PROBLEM_DEFINITION  
  TEMPORAL_DERIVATIVES:     ON  
  CONVECTIVE_TERM:          On
  VISCOUS_TERM:             DIVERGENCE
END_PROBLEM_DEFINITION  
END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT 
  ELEMENT_LENGTH:           Minimum
  STABILIZATION:            ASGS
  TRACKING:                 Time, CONVECTION, ITERAT=100, TOLER=1.0e-5, RELAX=1.0, ORDER=1, NEWTON
  TIME_INTEGRATION:         Trapezoidal, ORDER: 1
  SAFETY_FACTOR:            50.0
  STEADY_STATE_TOLER:       -1e-12

  NORM_OF_CONVERGENCE:      LAGGED_ALGEBRAIC_RESIDUAL
  MAXIMUM_NUMBER_OF_IT:     1
  CONVERGENCE_TOLERANCE:    1e-02

  ALGORITHM:                SCHUR
    SOLVER:                 ORTHOMIN, MOMENTUM_PRESERVING
    PRECONDITIONER:         TAU
    ELEMENT_LENGTH:         Minimum
    TAU_STRATEGY:           Codina
  END_ALGORITHM

  MOMENTUM
    ALGEBRAIC_SOLVER
      SOLVER:                GMRES, KRYLOV=10
      CONVERGENCE:           ITERA=1000, TOLER=1.0e-14, ADAPTIVE, RATIO=0.01
      PRECONDITIONER:        DIAGONAL
    END_ALGEBRAIC_SOLVER 
  END_MOMENTUM
  CONTINUITY 
    ALGEBRAIC_SOLVER
       SOLVER:               DEFLATED_CG 
       CONVERGENCE:          ITERA=1000, TOLER=1.0e-14, ADAPTIVE, RATIO=0.01
       PRECONDITIONER:       DIAGONAL
    END_ALGEBRAIC_SOLVER 
  END_CONTINUITY

END_NUMERICAL_TREATMENT 
$------------------------------------------------------------
OUTPUT_&_POST_PROCESS
$  START_POSTPROCESS_AT      STEP  = 0
  POSTPROCESS VELOCITY,     STEPS = 100
  POSTPROCESS PRESS,        STEPS = 100
  BOUNDARY_SET
    FORCE
  END_BOUNDARY_SET
END_OUTPUT_&_POST_PROCESS
$------------------------------------------------------------
BOUNDARY_CONDITIONS
  PARAMETERS
    INITIAL:   CONSTANT, VALUE=1.0,0.0,0.0
    VARIATION: NON_CONSTANT
  END_PARAMETERS
  CODES, NODES
      1 11 1.0 0.0
  1 & 2 11 1.0 0.0
$      1 11 0.0 0.0
$  1 & 2 11 0.0 0.0
      2 01 0.0 0.0
      3 11 0.0 0.0
  2 & 4 01 0.0 0.0
      4 00 0.0 0.0
  END_CODES
END_BOUNDARY_CONDITIONS  
$------------------------------------------------------------ 

