$------------------------------------------------------------
PHYSICAL_PROBLEM
$------------------------------------------------------------ 
PROBLEM_DEFINITION       
  TEMPORAL_DERIVATIVES:     On  
  CONVECTIVE_TERM:          On  
  VISCOUS_TERM:             laplacian
END_PROBLEM_DEFINITION  
$------------------------------------------------------------
END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT 
  STABILIZATION:            ASGS
  SAFETY_FACTOR:            500.0
  STEADY_STATE_TOLER:       1e-8   
  NORM_OF_CONVERGENCE:      Algebraic  
  LINEARIZATION_METHOD:     Picard   
  MAXIMUM_NUMBER_OF_IT:     1
  CONVERGENCE_TOLERANCE:    1e-8 

  VECTORIZATION: ON
  ALGORITHM:                SCHUR
    SOLVER:                 ORTHOMIN, MOMENTUM_PRESERVING
    PRECONDITIONER:         TAU
    ELEMENT_LENGTH:         Minimum
    TAU_STRATEGY:           Codina
  END_ALGORITHM

  MOMENTUM
    ALGEBRAIC_SOLVER     
      SOLVER:                GMRES, KRYLOV=10
      CONVERGENCE:           ITERA=1, TOLER=1.0e-10, ADAPTIVE, RATIO=1.0e-2
      OUTPUT:                CONVERGENCE
      PRECONDITIONER:        DIAGONAL
    END_ALGEBRAIC_SOLVER        
  END_MOMENTUM

  CONTINUITY 
    ALGEBRAIC_SOLVER
       SOLVER:               CG
       CONVERGENCE:          ITERA=1, TOLER=1.0e-12
       OUTPUT:               CONVERGENCE
       PRECONDITIONER:       DIAGONAL
    END_ALGEBRAIC_SOLVER        
  END_CONTINUITY

END_NUMERICAL_TREATMENT  
