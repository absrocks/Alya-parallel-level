$-------------------------------------------------------------------
PHYSICAL_PROBLEM
  PROPERTIES
    MATERIAL=1
      VISCOSITY:      CONSTANT,  PARAMETERS = 1.0
      DENSITY:        CONSTANT,  PARAMETERS = 1.0
      CONDUCTIVITY:   CONSTANT,  PARAMETERS = 1.0
      SPECIFIC_HEAT:  CONSTANT,  PARAMETERS = 1.0
    END_MATERIAL
  END_PROPERTIES
END_PHYSICAL_PROBLEM
$-------------------------------------------------------------------
NUMERICAL_TREATMENT    
  MESH
   DIVISION=0
  END_MESH
  ELSEST
    STRATEGY: BIN
    NUMBER:   5,5
    DATAF: LINKED_LIST
  END_ELSEST
  SPACE_&_TIME_FUNCTIONS
    FUNCTION=INFLOW, DIMENSION=2
       2.0*x+y
      -x-3.0*y
    END_FUNCTION
  END_SPACE_&_TIME_FUNCTIONS
END_NUMERICAL_TREATMENT  
$------------------------------------------------------------
OUTPUT_&_POST_PROCESS  
  STEPS=1e6
  ON_LAST_MESH
END_OUTPUT_&_POST_PROCESS  
$------------------------------------------------------------
 
