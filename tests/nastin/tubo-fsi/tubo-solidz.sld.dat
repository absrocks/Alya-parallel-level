$ IMPLICIT
$
NUMERICAL_TREATMENT
  TIME_TREATMENT:    IMPLICIT
  TIME_INTEGRATION:  NEWMARK, DAMPED
  ALGEBRAIC_SOLVER:
     SOLVER:               DEFLATEDCG, COARSE: SPARSE
     CONVERGENCE:          ITERA=1000, TOLER=1.0e-10, ADAPTIVE, RATIO=0.01
     OUTPUT:               CONVERGENCE
     PRECONDITIONER:       BLOCK_DIAGONAL
     OPTIONS:              ZERO_FIXITY
  END_ALGEBRAIC_SOLVER
  MAXIMUM_ITERATION:       500
  RESIDUAL:                TOTAL
  CONVERGENCE_TOLER:       1.00E-5
  SAFETY_FACTOR:           40.0
  STEADY_STATE_TOLERANCE: -1E-10
  ROTATION:                ON
END_NUMERICAL_TREATMENT

