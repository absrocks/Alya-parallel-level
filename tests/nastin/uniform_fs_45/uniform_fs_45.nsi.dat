$------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION       
    TEMPORAL_DERIVATIVES:   ON
    CONVECTIVE_TERM:        EMAC
    VISCOUS_TERM:           DIVERGENCE
  END_PROBLEM_DEFINITION  
END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT 
  STABILIZATION:            OFF
  TIME_INTEGRATION:         RUNGE, ORDER:4
  STEADY_STATE_TOLER        1e-10
  MAXIMUM_NUMBER_OF_IT:     1
  SAFETY_FACTOR:            1.0
  TIME_STRATEGY:            EIGENVALUE
  ASSEMBLY:                 GPU2
  GRAD_DIV =                ON

  ALGORITHM:                FRACTIONAL_STEP
  END_ALGORITHM

  MOMENTUM
     ALGEBRAIC_SOLVER
     SOLVER:                EXPLICIT, LUMPED
     END_ALGEBRAIC_SOLVER 
  END_MOMENTUM
  CONTINUITY
    ALGEBRAIC_SOLVER
       SOLVER:              DEFLATED_CG
       CONVERGENCE:         ITERA=1000, TOLER=-1.0e-10, ADAPTIVE, RATIO=1.0e-3
       PRECONDITIONER:      DIAGONAL
    END_ALGEBRAIC_SOLVER   
  END_CONTINUITY

  DIRICHLET: ALGORITHM
END_NUMERICAL_TREATMENT  
$------------------------------------------------------------ 
OUTPUT_&_POST_PROCESS  
  START_POSTPROCES_AT       STEP  = 0
  POSTPROCESS PRESS,        STEPS = 1
  POSTPROCESS VELOC,        STEPS = 1
  WITNESS_POINTS
     VELOX
     VELOY
  END_WITNESS_POINTS
END_OUTPUT_&_POST_PROCESS    
$------------------------------------------------------------
BOUNDARY_CONDITIONS
  PARAMETERS
    VARIATION:    NON_CONSTANT
    INITIAL:      CONSTANT, VALUE=0.5,0.5
  END_PARAMETERS
  CODES, NODES
        1  11  0.5  0.5
    1 & 2  11  0.5  0.5
        2  10  0.0  0.0, AXES: FROM_FIELD=1
    3 & 2  10  0.0  0.0, AXES: FROM_FIELD=1
  END_CODES  
END_BOUNDARY_CONDITIONS  
$------------------------------------------------------------
