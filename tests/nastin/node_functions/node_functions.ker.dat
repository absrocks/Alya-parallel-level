$-------------------------------------------------------------------
PHYSICAL_PROBLEM
  PROPERTIES
    MATERIAL: 1,     NAME=GENERIC
      DENSITY=       CONSTANT, VALUE=1.0
      VISCOSITY=     CONSTANT, VALUE=1.0
      CONDUCTIVITY=  CONSTANT, VALUE=1.0
      SPECIFIC_HEAT= CONSTANT, VALUE=1.0
    END_MATERIAL
  END_PROPERTIES
END_PHYSICAL_PROBLEM
$-------------------------------------------------------------------
NUMERICAL_TREATMENT 
  ELSEST
    STRATEGY:    Bin
    NUMBER_BINS= 20,20,20
  END_ELSEST
  MESH
    MULTIPLICATION= 0
  END_MESH
  SPACE_&_TIME_FUNCTIONS
    FUNCTION=VELOCITY, DIMENSION=2
        t+4
      3.0*t+3
    END_FUNCTION
    FUNCTION=TEMPERATURE
        t+1
    END_FUNCTION
  END_SPACE_&_TIME_FUNCTIONS
END_NUMERICAL_TREATMENT  
$-------------------------------------------------------------------
OUTPUT_&_POST_PROCESS  
  On_last_mesh
   STEPS = 1
END_OUTPUT_&_POST_PROCESS  
$-------------------------------------------------------------------
