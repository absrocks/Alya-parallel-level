$------------------------------------------------------------
PHYSICAL_PROBLEM
$------------------------------------------------------------ 
PROBLEM_DEFINITION       
  TEMPORAL_DERIVATIVES:     On 
  CONVECTIVE_TERM:          On
  VISCOUS_TERM:             LAPLACIAN
  GRAVITY:                  NORM= 1.0, GX=1.0,GY=0.0
END_PROBLEM_DEFINITION  
$------------------------------------------------------------
END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT 
  ELEMENT_LENGTH:           Minimum
  STABILIZATION:            galerkin
  STEADY_STATE_TOLER:       -1e-12

  NORM_OF_CONVERGENCE:      LAGGED_ALGEBRAIC_RESIDUAL
  MAXIMUM_NUMBER_OF_IT:     1
  CONVERGENCE_TOLERANCE:    1e-3

  VECTORIZED_ASSEMBLY:      ON

$  SAFETY_FACTOR:            100.0
$  TIME_INTEGRATION:         Trapezoidal, ORDER: 1, EULER=20
$  ALGORITHM:                SCHUR
$    SOLVER:                 ORTHOMIN, CONTINUITY_PRESERVING
$    PRECONDITIONER:         TAU
$    ELEMENT_LENGTH:         Minimum
$    TAU_STRATEGY:           Codina
$  END_ALGORITHM

  SAFETY_FACTOR:            0.1
  TIME_INTEGRATION:         ADAMS, ORDER: 2
  ALGORITHM:                FRACTIONAL_STEP
    GAMMA=                  0.0
  END_ALGORITHM

  MOMENTUM
    ALGEBRAIC_SOLVER     
      SOLVER:                GMRES, KRYLOV=10
      CONVERGENCE:           ITERA=1000, TOLER=1.0e-12, ADAPTIVE, RATIO=0.01
      OUTPUT:                CONVERGENCE
      PRECONDITIONER:        DIAGONAL
    END_ALGEBRAIC_SOLVER        
  END_MOMENTUM
  CONTINUITY 
    ALGEBRAIC_SOLVER
       SOLVER:               DEFLATED_CG
       CONVERGENCE:          ITERA=1000, TOLER=1.0e-12, ADAPTIVE, RATIO=0.01
       OUTPUT:               CONVERGENCE
       PRECONDITIONER:       LINELET, TOLER=10.0
    END_ALGEBRAIC_SOLVER        
  END_CONTINUITY
 
  DIRICHLET: MATRIX

END_NUMERICAL_TREATMENT  
$------------------------------------------------------------
OUTPUT_&_POST_PROCESS  
  postprocess VELOC,   steps=1e6
  postprocess PRESS,   steps=1e6
$  postprocess LINELET, steps=1e6
END_OUTPUT_&_POST_PROCESS   
$------------------------------------------------------------
BOUNDARY_CONDITIONS
  PARAMETERS
    FIX_PRESSURE: On, ON_NODE=4, VALUE=0.0
    INITIAL:      SPACE_TIME_FUNCTION: INITIAL_VELOCITY 
    VARIATION:    NON_CONSTANT
  END_PARAMETERS 
  CODES, NODES
      1  11  0.0 0.0
  END_CODES
END_BOUNDARY_CONDITIONS  
$------------------------------------------------------------
