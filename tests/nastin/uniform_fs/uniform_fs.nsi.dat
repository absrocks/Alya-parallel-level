$------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION       
    TEMPORAL_DERIVATIVES:   ON
    CONVECTIVE_TERM:        SKEW_SYMMETRIC
    VISCOUS_TERM:           LAPLACIAN
  END_PROBLEM_DEFINITION  
END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT 
  ELEMENT_LENGTH:           Minimum
  STEADY_STATE_TOLER:       1e-12

  VECTORIZED_ASSEMBLY:      ON

  NORM_OF_CONVERGENCE:      LAGGED_ALGEBRAIC_RESIDUAL
  CONVERGENCE_TOLERANCE:    1e-03
$  TIME_STEP_STRATEGY:       EIGENVALUE

  ALGORITHM:                FRACTIONAL_STEP
    GAMMA=                  1.0
  END_ALGORITHM
  MAXIMUM_NUMBER_OF_IT:     1
  STABILIZATION:            ASGS
  TIME_INTEGRATION:         ADAMS_BASHFORTH, ORDER=1
  SAFETY_FACTOR:            0.3

$  ALGORITHM:                SCHUR
$    SOLVER:                 ORTHOMIN, MOMENTUM_PRESERVING
$    PRECONDITIONER:         TAU
$    ELEMENT_LENGTH:         Minimum
$    TAU_STRATEGY:           Codina
$  END_ALGORITHM
$  MAXIMUM_NUMBER_OF_IT:     1
$  STABILIZATION:            ASGS
$  TIME_INTEGRATION:         BDF, ORDER=1
$  SAFETY_FACTOR:            10.0

  MOMENTUM
    ALGEBRAIC_SOLVER     
      SOLVER:               GMRES, KRYLOV=10
      CONVERGENCE:          ITERA=1000, TOLER=1.0e-14, ADAPTIVE, RATIO=0.01
      OUTPUT:               CONVERGENCE
      PRECONDITIONER:       DIAGONAL
    END_ALGEBRAIC_SOLVER       
  END_MOMENTUM
  CONTINUITY 
    ALGEBRAIC_SOLVER
       SOLVER:              DEFLATED_CG, COARSE: SPARSE
       CONVERGENCE:         ITERA=1000, TOLER=1.0e-14, ADAPTIVE, RATIO=0.01
       OUTPUT:              CONVERGENCE
       PRECONDITIONER:      DIAGONAL
    END_ALGEBRAIC_SOLVER        
  END_CONTINUITY

$  DIRICHLET: MATRIX
END_NUMERICAL_TREATMENT  
$------------------------------------------------------------ 
OUTPUT_&_POST_PROCESS  
  START_POSTPROCES_AT       STEP  = 0
  POSTPROCESS PRESS,        STEPS = 1
  POSTPROCESS VELOC,        STEPS = 1
  WITNESS_POINTS
     VELOX
     VELOY
  END_WITNESS_POINTS
END_OUTPUT_&_POST_PROCESS    
$------------------------------------------------------------
BOUNDARY_CONDITIONS
  PARAMETERS
    VARIATION:    NON_CONSTANT
    INITIAL:      CONSTANT, VALUE=1.0,0.0
  END_PARAMETERS
  CODES, NODES
        1  11  1.0  0.0
        2  10  0.0  0.0, AXES: LOCAL
$        2  01  0.0  0.0
    1 & 2  11  1.0  0.0
    3 & 2  01  0.0  0.0
  END_CODES  
END_BOUNDARY_CONDITIONS  
$------------------------------------------------------------
