$------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION       
    TEMPORAL_DERIVATIVES:   ON
    CONVECTIVE_TERM:        EMAC
    VISCOUS_TERM:           DIVERGENCE
  END_PROBLEM_DEFINITION  
END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT 
  ELEMENT_LENGTH:           Minimum
  STABILIZATION:            OFF

  TIME_INTEGRATION:         RUNGE_KUTTA, ORDER:3
  TIME_STEP:                EIGENVALUE
  ASSEMBLY:                 GPU2
  GRAD_DIV:                 ON

  SAFETY_FACTOR:            0.7

  STEADY_STATE_TOLER:       -1e-12

  NORM_OF_CONVERGENCE:      LAGGED_ALGEBRAIC_RESIDUAL
  MAXIMUM_NUMBER_OF_IT:     1
  CONVERGENCE_TOLERANCE:    1e-3

  ALGORITHM:                FRACTIONAL_STEP
    GAMMA=                  0.0
  END_ALGORITHM

  MOMENTUM
    ALGEBRAIC_SOLVER     
      SOLVER:              EXPLICIT, CONSISTENT
    END_ALGEBRAIC_SOLVER       
  END_MOMENTUM
  CONTINUITY 
    ALGEBRAIC_SOLVER
       SOLVER:              DEFLATED_CG, COARSE: SPARSE
       CONVERGENCE:         ITERA=1000, TOLER=1.0e-14, ADAPTIVE, RATIO=0.01
       OUTPUT:              CONVERGENCE
       PRECONDITIONER:      DIAGONAL
    END_ALGEBRAIC_SOLVER        
  END_CONTINUITY

  CONSISTENT_MASS
    ALGEBRAIC_SOLVER
       SOLVER:              CG, COARSE: SPARSE
       CONVERGENCE:         ITERA=1000, TOLER=1.0e-14, ADAPTIVE, RATIO=1.0e-10
       OUTPUT:              CONVERGENCE
       PRECONDITIONER:      DIAGONAL
    END_ALGEBRAIC_SOLVER        
  END_CONSISTENT_MASS

  DIRICHLET: ALGORITHM

END_NUMERICAL_TREATMENT  
$------------------------------------------------------------ 
OUTPUT_&_POST_PROCESS  
  START_POSTPROCES_AT       STEP  = 0
  POSTPROCESS PRESS,        STEPS = 1
  POSTPROCESS VELOC,        STEPS = 1
  WITNESS_POINTS
     VELOX
     VELOY
  END_WITNESS_POINTS
END_OUTPUT_&_POST_PROCESS    
$------------------------------------------------------------
BOUNDARY_CONDITIONS
  PARAMETERS
    VARIATION:    NON_CONSTANT
    INITIAL:      CONSTANT, VALUE=0.8,0.0
  END_PARAMETERS
  CODES, NODES
        1  11  1.0  0.0
$        2  10  0.0  0.0, AXES: LOCAL
        2  01  0.0  0.0
    1 & 2  11  1.0  0.0
    3 & 2  01  0.0  0.0
  END_CODES  
END_BOUNDARY_CONDITIONS  
$------------------------------------------------------------
