$------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION       
    TEMPORAL_DERIVATIVES:     ON 
    CONVECTIVE_TERM:          NONCO
    VISCOUS_TERM:             FULL
    REGIME=                   LOWMACH,   TEMPERATURE = 273.0
    GRAVITY:                  NORM=0.00295908836019, GX=0.0, GY=-1.0 
  END_PROBLEM_DEFINITION  

  PROPERTIES
    THERMODYN_PRESSURE = 101325.0, MASS_CONSERVATION
  END_PROPERTIES

END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT
  STABILIZATION:            OFF
  ELEMENT_LENGTH:           MINIMUM
  TIME_INTEGRATION:         RUNGE, ORDER = 3
  SAFETY_FACTOR             0.9

  STEADY_STATE_TOLER:       -1e-10
  MAXIMUM_NUMBER_OF_IT:     1
  CONVERGENCE_TOLERANCE:    1e-4
  NORM_OF_CONVERGENCE       LAGGED_ALGEBRAIC_RESIDUAL

  VECTORIZED_ASSEMBLY:      ON
  TIME_STRATEGY:            EIGENVALUE

  ALGORITHM:                FRACTIONAL_STEP
    GAMMA = 0.0
    ROTAT = 0.0
  END_ALGORITHM

  MOMENTUM
    ALGEBRAIC_SOLVER
      SOLVER:               EXPLICIT, LUMPED
    END_ALGEBRAIC_SOLVER
  END_MOMENTUM

  CONTINUITY
    ALGEBRAIC_SOLVER
      SOLVER:               DEFLATED_CG, COARSE: SPARSE
      CONVERGENCE:          ITERA=300, TOLER=1.0e-10, ADAPTIVE, RATIO=0.001
      PRECONDITIONER:       DIAGONAL
    END_ALGEBRAIC_SOLVER
  END_CONTINUITY

  DIRICHLET : ALGORITHM

END_NUMERICAL_TREATMENT 
$------------------------------------------------------------
OUTPUT_&_POST_PROCESS
$  START_POSTPROCES_AT       STEP = 0
  POSTPROCESS VELOC
END_OUTPUT_&_POST_PROCESS  
$------------------------------------------------------------
BOUNDARY_CONDITIONS
  PARAMETERS
     INITIAL:        CONSTANT= 0.0 0.0
     FIX_PRESSURE:   ON, ON_NODE= 1, VALUE= 0.0
  END_PARAMETERS

  CODES, NODES 
       1    11  0.00      0.00    $$ Bottom
       2    11  0.00      0.00    $$ Right
       3    11  0.00      0.00    $$ Top
       4    11  0.00      0.00    $$ Left
     1 & 2  11  0.00      0.00
     1 & 4  11  0.00      0.00
     2 & 3  11  0.00      0.00
     3 & 4  11  0.00      0.00
  END_CODES
END_BOUNDARY_CONDITIONS 
