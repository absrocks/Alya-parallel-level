$------------------------------------------------------------
PHYSICAL_PROBLEM
$------------------------------------------------------------ 
PROBLEM_DEFINITION       
  TEMPORAL_DERIVATIVES:     On 
  CONVECTIVE_TERM:          emac
  VISCOUS_TERM:             divergence
END_PROBLEM_DEFINITION  
$------------------------------------------------------------
PROPERTIES
END_PROPERTIES  
$------------------------------------------------------------
END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT 
  ELEMENT_LENGTH:           Minimum
  STABILIZATION:            Off
  TIME_INTEGRATION:         RUNGE, ORDER: 2
  SAFETY_FACTOR:            2.0
  STEADY_STATE_TOLER:       -1e-12
  TIMES:                    EIGEN

  VECTORIZED_ASSEMBLY:      ON
  ASSEMBLY:                 GPU2
  GRAD_DIV:                 ON

  NORM_OF_CONVERGENCE:      LAGGED_ALGEBRAIC_RESIDUAL
  MAXIMUM_NUMBER_OF_IT:     1
  CONVERGENCE_TOLERANCE:    1e-5

  ALGORITHM:                SEMI_IMPLICIT 
  END_ALGORITHM

  MOMENTUM
     ALGEBRAIC_SOLVER
     SOLVER:                EXPLICIT, LUMPED
     END_ALGEBRAIC_SOLVER 
  END_MOMENTUM
  CONTINUITY 
    ALGEBRAIC_SOLVER
       SOLVER:              DEFLATED_CG, COARSE: SPARSE
       CONVERGENCE:         ITERA=1000, TOLER=1.0e-14, ADAPTIVE, RATIO=0.01
       OUTPUT:              CONVERGENCE
       PRECONDITIONER:      DIAGONAL
    END_ALGEBRAIC_SOLVER       
  END_CONTINUITY
  VISCOUS_TERM 
    ALGEBRAIC_SOLVER
       SOLVER:              CG, COARSE: SPARSE
       CONVERGENCE:         ITERA=1000, TOLER=1.0e-14, ADAPTIVE, RATIO=0.01
       OUTPUT:              CONVERGENCE
       PRECONDITIONER:      DIAGONAL
    END_ALGEBRAIC_SOLVER        
  END_VISCOUS_TERM


END_NUMERICAL_TREATMENT  
$------------------------------------------------------------
OUTPUT_&_POST_PROCESS  
  POSTPROCESS VELOCITY,     STEPS = 1
  POSTPROCESS PRESSURE,     STEPS = 1
END_OUTPUT_&_POST_PROCESS   
$------------------------------------------------------------
BOUNDARY_CONDITIONS
  PARAMETERS
    FIX_PRESSURE: On, ON_NODE=121, VALUE=0.0
  END_PARAMETERS
  CODES, NODES
        1  11  1.000000  0.000000 
        2  11  0.000000  0.000000 
    1 & 2  11  0.000000  0.000000 
  END_CODES
END_BOUNDARY_CONDITIONS  
$------------------------------------------------------------
