PHYSICAL_PROBLEM
  PROPERTIES
    MATERIAL = 1
      DENSITY:   CONSTANT: VALUE=1.0
      VISCOSITY: CONSTANT: VALUE=0.01
      $CONDU: CONSTANT: VALUE=2.8594245e-02
      $SPECI: CONSTANT: VALUE=1004.0
      $TURBULENCE: ILSA, PARAMETERS=476.19048, 0.02, UPDP1=ENDSTE, UPDP2=NEVER
      $TURBULENCE: VRMAN, PARAMETERS=0.003703, UPDP1=ENDSTE, UPDP2=NEVER
    END_MATERIAL
  END_PROPERTIES
END_PHYSICAL_PROBLEM

NUMERICAL_TREATMENT
  MESH:
    MASS: APPROXIMATE_INVERSE
    MASS: CONSISTENT
  END_MESH
  ELSEST
    STRATEGY: BIN
    NUMBER:   10,10,10
    DATAF:    LINKED_LIST
    TOLER:    1e-10
  END_ELSEST
  HESSIAN:   OFF
  LAPLACIAN: OFF
END_NUMERICAL_TREATMENT

OUTPUT_&_POST_PROCESS
  ON_LAST_MESH
  STEP_OVER: 100
  POSTPROCESS VELOCITY
  POSTPROCESS PRESSURE
  $POSTPROCESS TURBULENCE
  $POSTPROCESS TEMPERATURE

  $WITNESS_POINTS, NUMBER = 3000
  $  INCLUDE ./mesh/witness.dat
  $END_WITNESS_POINTS

END_OUTPUT_&_POST_PROCESS
