$------------------------------------------------------------
PHYSICAL_PROBLEM 
$------------------------------------------------------------
PROBLEM_DEFINITION
  TEMPORAL         DYNAMIC
END_PROBLEM_DEFINITION

PROPERTIES

   DENSITY=              1.0
   LAW_DENSITY=         CONSTANT
 
   RAYLEIGH_DUMPING, RAYALPHA= 200.0
   CONSTITUTIVE_MODEL   HOLZAPFEL    \
                        5.0e4 1000000.0 830.0 6.710 30050.0 5.972 5010.0 3.439 710.0 3.982 1.0
    COUPLING_SCHEME
        MODEL: HUNTER $LAND
        CONTROL_FACTOR: 0.5
        CAL50:          0.5
    END_COUPLING_SCHEME


FIBER_MODEL
   LOCATION: NODAL
   FIBER:    1
END_FIBER_MODEL

END_PROPERTIES 

$------------------------------------------------------------
END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT   


$  TIME_TREATMENT:   EXPLICIT
$  TIME_INTEGRATION: NEWMARK, DAMPED
$  SAFETY_FACTOR: 0.1

  STEADY_STATE_TOLERANCE: -1.0

  SAFETY_FACTOR: 5.0

  TIME_TREATMENT:   IMPLICIT
  TIME_INTEGRATION: NEWMARK, DAMPED

  RESIDUAL: DISP 
  CONVERGENCE_TOLER: 1e-3

  MAXIMUM_ITERATION: 100

  ALGEBRAIC_SOLVER:
    SOLVER: CG
    CONVERGENCE: ITERA = 10000, TOLER = 1.0e-8, ADAPTIVE, RATIO = 0.01
    OUTPUT: CONVERGENCE
    PRECONDITIONER: BLOCK_DIAGONAL
    OPTIONS: ZERO_FIXITY
  END_ALGEBRAIC_SOLVER

  ROTATION:          ON
END_NUMERICAL_TREATMENT
$------------------------------------------------------------
OUTPUT_&_POST-PROCESS 
    START_POSTPROCESS_AT STEP=50
 
  POSTPROCESS CALCIUM   STEPS= 50

  WITNESS_POINTS
    DISPX
    DISPY
    DISPZ
  END_WITNESS_POINTS


END_OUTPUT_&_POST_PROCESS  
$------------------------------------------------------------
BOUNDARY_CONDITIONS
$------------------------------------------------------------


CODES, NODES

       1     000 0.0 0.0 0.0  $$ CONTACT
       3     001 0.0 0.0 0.0  $$ BASE
    1  &  3  001 0.0 0.0 0.0


END_CODES

END_BOUNDARY_CONDITIONS  
$------------------------------------------------------------
