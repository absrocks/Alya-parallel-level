$-------------------------------------------------------------------
RUN_DATA
  ALYA:                  pzt-actuator_repart
  CODE=                  1
  LIVE_INFO:             SCREEN
  LOG_FILE:              ON
END_RUN_DATA
$-------------------------------------------------------------------
PROBLEM_DATA
$  TIME_COUPLING:         GLOBAL, FROM_CRITICAL
  TIME_COUPLING:         GLOBAL, PRESCRIBED
  TIME_INTERVAL:         0.0, 1.1E-4
  TIME_STEP_SIZE=        1.0E-6
  NUMBER_OF_STEPS=       20 $110
  MAXIMUM_NUMBER_GLOBAL= 1
  $------------------------------------
  SOLIDZ_MODULE:         ON
  END_SOLIDZ_MODULE
  PARALL_SERVICE:        ON
    POSTPROCESS:         MASTER
    PARTITION_TYPE:      FACES
    PARTITIONING
      METHOD:            RANDOM
      EXECUTION_MODE:    PARALLEL
    END_PARTITIONING
    REPARTITIONING:      ON
$      MODULE:           SOLIDZ
$      THRESHOLD_LOAD_BALANCE= 0.9
$      CRITERION:         ASSEMBLY
      CRITERION:         ALWAYS
      FREQUENCY=         1
      POSTPROCESS:       NONE
    END_REPARTITIONING
    ORDER_COMMUNICATIONS: ON
    OPENMP
      ELEMENT_CHUNK=     100
      COLORING:          MIN_COLOR
    END_OPENMP
  END_PARALL_SERVICE
END_PROBLEM_DATA
$-------------------------------------------------------------------
