$-------------------------------------------------------------------
$
$ indenter
$ Details:   Indenter in 3-d
$            Dimensions: R=; W=; h=
$            Indenter is assumed as a rigid body
$ Units:     -
$
$ Reference: Results are compared against Code Aster
$            Full case:
$            Displacement indenter:
$            Total time steps:
$
$-------------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION
    TEMPORAL_DERIVATIVES: DYNAMIC
  END_PROBLEM_DEFINITION
  PROPERTIES
    MATERIAL         =  1
    DENSITY          =  1.0E+5
    CONSTITUTIVE_MODEL: ISOLIN \
      68.96E+7 0.32
  END_PROPERTIES
END_PHYSICAL_PROBLEM
$-------------------------------------------------------------------
NUMERICAL_TREATMENT
  TIME_TREATMENT:      EXPLICIT
  TIME_INTEGRATION:    NEWMARK, CDIFF
  NO_STEADY_STATE
  SAFETY_FACTOR=       0.85
  MASS_SCALING=        1.0
END_NUMERICAL_TREATMENT
$-------------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  START_POSTPROCESS_AT STEP = 0
  POSTPROCESS DISPL
END_OUTPUT_&_POST_PROCESS
$-------------------------------------------------------------------
BOUNDARY_CONDITIONS, TRANSIENT
  CODES, NODES
        1 111 0.0 0.0 0.0, MODULE_FUNCTION= 1
    1 & 2 111 0.0 0.0 0.0, MODULE_FUNCTION= 1
    1 & 3 111 0.0 0.0 0.0, MODULE_FUNCTION= 1
  END_CODES
  PARAMETERS
    CONTACT_PDN
      UNILATERAL
      BOUNDARY:   2
      PROJECTION: Y
    END_CONTACT_PDN
  END_PARAMETERS
  FUNCTIONS
    TOTAL_NUMBER = 1
    CONDITION
      FUNCTION_NUMBER: 1
      TIME_SHAPE: DISCRETE, SMOOTH
      SHAPE_DEFINITION
        2
        0.0  0.0  0.0  0.0
        0.05 0.0 -0.03 0.0
      END_SHAPE_DEFINITION
    END_CONDITION
  END_FUNCTIONS
END_BOUNDARY_CONDITIONS
$-------------------------------------------------------------------
