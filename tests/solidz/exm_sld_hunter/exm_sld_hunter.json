            },

            "mpi": 1,
            "openmp": 0
        },
        {
            "commands": {
                "alya": "[RUN] [NP] [MPI] [ALYA] --write-rst --time-steps 100 [NAME] ; [RUN] [NP] [MPI] [ALYA] --read-rst --time-steps 200 [NAME]"
            },
            "mpi": 3,
            "openmp": 0
        }
    ],
    "modules": [
        "exmedi",
        "solidz"
    ],
    "name": "exm_sld_hunter",
    "postprocess": "alya2pos",
    "timeout": "00:05:00"
