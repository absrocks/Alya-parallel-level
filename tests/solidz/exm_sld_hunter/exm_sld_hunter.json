{
    "authors": [
        "alfonso.santiago@bsc.es",
        "adria.quintanas@bsc.es"
    ],
    "comparisons": [
        {
            "file": "exm_sld_hunter-node.exm.set",
            "method": "absolute",
            "tolerance": "1.0e-4"
        },
        {
            "file": "exm_sld_hunter-node.sld.set",
