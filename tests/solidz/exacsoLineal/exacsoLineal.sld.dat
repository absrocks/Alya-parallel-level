$------------------------------------------------------------
PHYSICAL_PROBLEM
  PROBLEM_DEFINITION
    TEMPORAL_DERIVATIVES: DYNAMIC
  END_PROBLEM_DEFINITION
  PROPERTIES
    DENSITY=              1E+4
    CONSTITUTIVE_MODEL    ISOLIN  200e9 0.3
  END_PROPERTIES
END_PHYSICAL_PROBLEM
$------------------------------------------------------------
NUMERICAL_TREATMENT
  TIME_TREATMENT:         IMPLICIT
  TIME_INTEGRATION:       NEWMARK, DAMPED
  ALGEBRAIC_SOLVER
     SOLVER:              CG
     CONVERGENCE:         ITERA=1000, TOLER=1.0e-14, ADAPTIVE, RATIO=0.0001
     OUTPUT:              CONVERGENCE
     PRECONDITIONER:      DIAGONAL
  END_ALGEBRAIC_SOLVER
  MAXIMUM_ITERATION:      100
  STEADY_STATE_TOLERANCE: 1.0e-12
  CONVERGENCE_TOLER:      1.00E-12
  SAFETY_FACTOR:          10.0
END_NUMERICAL_TREATMENT
$------------------------------------------------------------
OUTPUT_&_POST_PROCESS
  START_POSTPROCESS_AT    STEP = 0
  POSTPROCESS DISPL       STEPS = 1
  POSTPROCESS ERROR       STEPS = 1
  OUTPUT      ERROR       SOLUTION = 2
END_OUTPUT_&_POST_PROCESS
$------------------------------------------------------------
BOUNDARY_CONDITIONS, CONSTANT
  CODES, NODES
    1      11 0.0 0.0
    2      11 0.0 0.0
    1 & 2  11 0.0 0.0
  END_CODES
END_BOUNDARY_CONDITIONS
$------------------------------------------------------------
